<script>
function auth_sele_change()
	if(window.opener.document.getElementById("encry__sele").options.selectedIndex == 0)
		window.opener.document.wireless_apcli_setup.encry_type[0].checked = true;
		window.opener.document.getElementById("encry_dis").disabled = false;
		window.opener.document.getElementById("encry_wep64").disabled = true;
		window.opener.document.getElementById("encry_wep128").disabled = true;
		window.opener.document.getElementById("encry_tkip").disabled = true;
		window.opener.document.getElementById("encry_aes").disabled = true;
		window.opener.document.getElementById("encry_tkip_aes").disabled = true;
		window.opener.document.getElementById("div_wep").style.visibility = "hidden";
		window.opener.document.getElementById("div_wep").style.display = "none";
		window.opener.document.getElementById("wep_key_5").style.visibility = "hidden";
		window.opener.document.getElementById("wep_key_5").style.display = "none";
		window.opener.document.getElementById("div_wpa").style.visibility = "hidden";
		window.opener.document.getElementById("div_wpa").style.display = "none";
	else if(window.opener.document.getElementById("encry__sele").options.selectedIndex == 1)
		window.opener.document.wireless_apcli_setup.encry_type[1].checked = true;
		window.opener.document.getElementById("encry_dis").disabled = true;
		window.opener.document.getElementById("encry_wep64").disabled = false;
		window.opener.document.getElementById("encry_wep128").disabled = false;
		window.opener.document.getElementById("encry_tkip").disabled = true;
		window.opener.document.getElementById("encry_aes").disabled = true;
		window.opener.document.getElementById("encry_tkip_aes").disabled = true;
		window.opener.document.getElementById("div_wep").style.visibility = "visible";
		window.opener.document.getElementById("div_wep").style.display = "";
		window.opener.document.getElementById("wep_key_5").style.visibility = "visible";
		window.opener.document.getElementById("wep_key_5").style.display = "";
		window.opener.document.getElementById("div_wpa").style.visibility = "hidden";
		window.opener.document.getElementById("div_wpa").style.display = "none";
	else if(window.opener.document.getElementById("encry__sele").options.selectedIndex == 2)
		window.opener.document.wireless_apcli_setup.encry_type[1].checked = true;
		window.opener.document.getElementById("encry_dis").disabled = true;
		window.opener.document.getElementById("encry_wep64").disabled = false;
		window.opener.document.getElementById("encry_wep128").disabled = false;
		window.opener.document.getElementById("encry_tkip").disabled = true;
		window.opener.document.getElementById("encry_aes").disabled = true;
		window.opener.document.getElementById("encry_tkip_aes").disabled = true;
		window.opener.document.getElementById("wep_key_5").style.visibility = "visible";
		window.opener.document.getElementById("wep_key_5").style.display = "";
		window.opener.document.getElementById("div_wep").style.visibility = "visible";
		window.opener.document.getElementById("div_wep").style.display = "";
		window.opener.document.getElementById("div_wpa").style.visibility = "hidden";
		window.opener.document.getElementById("div_wpa").style.display = "none";
	else if((window.opener.document.getElementById("encry__sele").options.selectedIndex == 3) )
		window.opener.document.wireless_apcli_setup.encry_type[3].checked = true;
		window.opener.document.getElementById("encry_dis").disabled = true;
		window.opener.document.getElementById("encry_wep64").disabled = true;
		window.opener.document.getElementById("encry_wep128").disabled = true;
		window.opener.document.getElementById("encry_tkip").disabled = false;
		window.opener.document.getElementById("encry_aes").disabled = false;
		window.opener.document.getElementById("encry_tkip_aes").disabled = true;
		window.opener.document.getElementById("wep_key_5").style.visibility = "hidden";
		window.opener.document.getElementById("wep_key_5").style.display = "none";
		window.opener.document.getElementById("div_wpa").style.visibility = "visible";
		window.opener.document.getElementById("div_wpa").style.display = "";
		window.opener.document.getElementById("div_wep").style.visibility = "hidden";
		window.opener.document.getElementById("div_wep").style.display = "none";
	else if( (window.opener.document.getElementById("encry__sele").options.selectedIndex == 4) || (window.opener.document.getElementById("encry__sele").options.selectedIndex == 5))
		window.opener.document.wireless_apcli_setup.encry_type[5].checked = true;
		window.opener.document.getElementById("encry_dis").disabled = true;
		window.opener.document.getElementById("encry_wep64").disabled = true;
		window.opener.document.getElementById("encry_wep128").disabled = true;
		window.opener.document.getElementById("encry_tkip").disabled = false;
		window.opener.document.getElementById("encry_aes").disabled = false;
		window.opener.document.getElementById("encry_tkip_aes").disabled = false;
		window.opener.document.getElementById("wep_key_5").style.visibility = "hidden";
		window.opener.document.getElementById("wep_key_5").style.display = "none";
		window.opener.document.getElementById("div_wpa").style.visibility = "visible";
		window.opener.document.getElementById("div_wpa").style.display = "";
		window.opener.document.getElementById("div_wep").style.visibility = "hidden";
		window.opener.document.getElementById("div_wep").style.display = "none";
function trim(str){ 
     return str.replace(/(^\s*)|(\s*$)/g, "");
function setvalue()
	var h;
	var tmpi,tmpj;
	var auth_str;
	var ext_channel;
	var sele_flag =0 ;
	var i = document.getElementById("aplist").rows.length;
	var tmp;
	if(i !=1)
	for(h=0;h<i;h++)
		if(document.scan_fm.selectap[h].checked == true)
			tmpi=h;
			tmpj=document.scan_fm.selectap[h].value;
			sele_flag = 1;
			break;
			}
	else if(i==1)
		if(document.scan_fm.selectap.checked == true)
			tmpi=0;
			tmpj=document.scan_fm.selectap.value;
			sele_flag = 1;
	if(!sele_flag)
		alert("
AP!");
		return;
	if(i >1)
		tmp = document.getElementById("ssid_"+tmpi).innerHTML;
		for(var x=0;x<32;x++){
			tmp = tmp.replace('&amp;','&');
			tmp = tmp.replace('&lt;','<');
			tmp = tmp.replace('&gt;','>');
			tmp = tmp.replace('&nbsp;',' ');
		window.opener.document.wireless_apcli_setup.apcli_ssid.value=tmp;
		window.opener.document.wireless_apcli_setup.apcli_bssid.value = tmpj;
		window.opener.document.wireless_apcli_setup.channel_sele_usa.options.selectedIndex = document.getElementById("channel_"+tmpi).innerHTML;
		auth_str=document.getElementById("type_"+tmpi).innerHTML;
		ext_channel=document.getElementById("uplow_"+tmpi).innerHTML;
	else if(i ==1)
		tmp = document.getElementById("ssid_0").innerHTML;
		for(var x=0;x<32;x++){
			tmp = tmp.replace('&amp;','&');
			tmp = tmp.replace('&lt;','<');
			tmp = tmp.replace('&gt;','>');
			tmp = tmp.replace('&nbsp;',' ');
		window.opener.document.wireless_apcli_setup.apcli_ssid.value=tmp;
		window.opener.document.wireless_apcli_setup.apcli_bssid.value=tmpj;
		window.opener.document.wireless_apcli_setup.channel_sele_usa.options.selectedIndex=document.getElementById("channel_0").innerHTML;
		auth_str=document.getElementById("type_0").innerHTML;
		ext_channel=document.getElementById("uplow_0").innerHTML;
	if(ext_channel == "Lower" || ext_channel == "LOWER")
		window.opener.document.getElementById("extChannel").selectedIndex = 1;
		window.opener.document.getElementById("extChannel").disabled = false;
	else if(ext_channel == "Upper" || ext_channel == "UPPER")
		window.opener.document.getElementById("extChannel").selectedIndex = 0;
		window.opener.document.getElementById("extChannel").disabled = false;
	else if(ext_channel == "none" || ext_channel == "NONE")
		window.opener.document.getElementById("extChannel").disabled = true;
	if(trim(auth_str)=="NONE")
		window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=0;
		auth_sele_change();
	else if(trim(auth_str)=="WEP/OPEN")
		window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=1;
		auth_sele_change();
		window.opener.document.wireless_apcli_setup.wep5_key1.focus();
	else if(trim(auth_str)=="WEP/SHARED")
		window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=2;
		auth_sele_change();
		window.opener.document.wireless_apcli_setup.wep5_key1.focus();
	else if(auth_str.search("WPA") != -1)
		if(auth_str.split("/")[0] =="WPAPSK" || auth_str.split("/")[0] =="WPA1PSK")
			window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=3;
			auth_sele_change();
			if(auth_str.split("/")[1] == "AES" )
			{
				window.opener.document.getElementById("encry_aes").checked = true;
			}
			else if(auth_str.split("/")[1] == "TKIP")
			{
				window.opener.document.getElementById("encry_tkip").checked = true;
			}
			window.opener.document.getElementById("wpa_pass").focus();
		else if(auth_str.split("/")[0] =="WPA2PSK" )
			window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=4;
			auth_sele_change();
			if(auth_str.split("/")[1] == "AES")
			{
				window.opener.document.getElementById("encry_aes").checked = true;
			}
			else if( auth_str.split("/")[1] == "TKIPAES")
			{
				window.opener.document.getElementById("encry_tkip_aes").checked = true;
			}
			else if(auth_str.split("/")[1] == "TKIP")
			{
				window.opener.document.getElementById("encry_tkip").checked = true;
			}
			window.opener.document.getElementById("wpa_pass").focus();
		else if(auth_str.split("/")[0] =="WPA1PSKWPA2PSK")
			window.opener.document.wireless_apcli_setup.authen_code_sele.options.selectedIndex=5;
			auth_sele_change();
			if(auth_str.split("/")[1] == "AES")
			{
				window.opener.document.getElementById("encry_aes").checked = true;
			}
			else if( auth_str.split("/")[1] == "TKIPAES")
			{
				window.opener.document.getElementById("encry_tkip_aes").checked = true;
			}
			else if(auth_str.split("/")[1] == "TKIP")
			{
				window.opener.document.getElementById("encry_tkip").checked = true;
			}
			window.opener.document.getElementById("wpa_pass").focus();
				
	window.close(); 
function init()
	var num = document.getElementById("aplist").rows.length;
	for(var j=0;j<num;j++)
		document.getElementById("ssid_"+j).innerHTML = document.getElementById("ssid_"+j).innerHTML.replace(eval("/ /gi"),'&nbsp;');
</script>
