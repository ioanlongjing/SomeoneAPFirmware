<SCRIPT language="JavaScript" type="text/javascript"> 
function CheckMAC(mac_addr)
	var	i, idx;
	for(i = 0; i < 5; i++)
		if(isNaN(parseInt(mac_addr.charAt(3*i), 16)))
			alert('The MAC address are worng format or using incorrect characters.');
		    //eval("document.wds.acl_mac"+no).focus();
		    //eval("document.wds.acl_mac"+no).select();
			return false;
		}	
		else if(isNaN(parseInt(mac_addr.charAt(3*i+1), 16)))
			alert('The MAC address are worng format or using incorrect characters.');
		    //eval("document.wds.acl_mac"+no).focus();
		    //eval("document.wds.acl_mac"+no).select();
		    return false;
		}    
		else if (mac_addr.charAt(3*i+2) != ':')
			alert('The MAC address are worng format or using incorrect characters.');
			//eval("document.wds.acl_mac"+no).focus();
			//eval("document.wds.acl_mac"+no).select();
			return false;     	
		}    
	if(isNaN(parseInt(mac_addr.charAt(15), 16)))
		alert('The MAC address are worng format or using incorrect characters.');
		//eval("document.wds.acl_mac"+no).focus();
		//eval("document.wds.acl_mac"+no).select();
		return false;
	else if(isNaN(parseInt(mac_addr.charAt(16), 16)))
		alert('The MAC address are worng format or using incorrect characters.');
		//eval("document.wds.acl_mac"+no).focus();
		//eval("document.wds.acl_mac"+no).select();
		return false;
	return true;
var phy_mode=Array("CCK","OFDM","HTMIX","GREENFIELD");
function add()
	var f=document.wds;
	var phy_index=f.wds_phy_mode.options.selectedIndex;
	var ap_bssid=f.ap_bssid1.value+":"+f.ap_bssid2.value+":"+f.ap_bssid3.value+":"+f.ap_bssid4.value+":"+f.ap_bssid5.value+":"+f.ap_bssid6.value;
	if(!CheckMAC(ap_bssid))
		return;
	if(total_wds==0)
		f.WdsList2.value=ap_bssid+";;;";
		f.WdsPhyMode2.value=phy_mode[phy_index]+";;;";
	else if(total_wds==1)
		f.WdsList2.value=str.split(";")[0]+";"+ap_bssid+";;";
		f.WdsPhyMode2.value=str1.split(";")[0]+";"+phy_mode[phy_index]+";;";
	else if(total_wds==2)
		f.WdsList2.value=str.split(";")[0]+";"+str.split(";")[1]+";"+ap_bssid+";";
		f.WdsPhyMode2.value=str1.split(";")[0]+";"+str1.split(";")[1]+";"+phy_mode[phy_index]+";";
	else if(total_wds==3)
		f.WdsList2.value=str.split(";")[0]+";"+str.split(";")[1]+";"+str.split(";")[2]+";"+ap_bssid;
		f.WdsPhyMode2.value=str1.split(";")[0]+";"+str1.split(";")[1]+";"+str1.split(";")[2]+";"+phy_mode[phy_index];
	else
	     alert("The Max number of AP is 4!");
		 return;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WDS',cPage);
	return true;
function check()
	var f=document.wds;
	//for(i=0;i<2;i++)
		if(!IsHex(f.ap_bssid1.value.split("")[i]))
			alert("1Invalid MAC Address."+f.ap_bssid1.value.split("")[i]);
			return;
		if(!IsHex(f.ap_bssid2.value.split("")[i]))
			alert("2Invalid MAC Address."+f.ap_bssid2.value.split("")[i]);
			return;
		/*
		if(!IsHex(f.ap_bssid3.value.split("")[i]))
			alert("3Invalid MAC Address."+f.ap_bssid3.value.split("")[i]);
			return;
		if(!IsHex(f.ap_bssid4.value.split("")[i]))
			alert("4Invalid MAC Address."+f.ap_bssid4.value.split("")[i]);
			return;
		if(!IsHex(f.ap_bssid5.value.split("")[i]))
			alert("5Invalid MAC Address."+f.ap_bssid5.value.split("")[i]);
			return;
		if(!IsHex(f.ap_bssid6.value.split("")[i]))
			alert("6Invalid MAC Address."+f.ap_bssid6.value.split("")[i]);
			return;
		*/
function delect_all()
	if(!total_wds)
		return;
	document.wds.WdsList2.value=";;;";
	var f=document.wds;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WDS',cPage);
	return true;
