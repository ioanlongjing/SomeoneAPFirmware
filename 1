var dhs=getCfg("DHS").match(/\d{1,3}$/);
var dhe=getCfg("DHE").match(/\d{1,3}$/);
var s_edit_idx;
var SelRowBC;
var SelRow = -1;
function trim(str){ //?
return str.replace(/(^\s*)|(\s*$)/g, ""); 
function digit(s) 
	var i;
	for( i = 0 ; i < s.length; i++ )
		if( !((s.charAt(i) >= '0' ) && (s.charAt(i) <= '9')) )
			return 1;
	return 0;
function init()
	var f=document.frmSetup;
	f.dips.value=f.dips.defaultValue=getCfg("DHS").match(/\d{1,3}$/);
	f.dipe.value=f.dipe.defaultValue=getCfg("DHE").match(/\d{1,3}$/);
	cfg2Form(f);
	dyn_sel_idx=-1;
	if(dhcpList.length > 0)
		document.getElementById("ip_mac_en").checked = true;
	SelectRow("tab",-1);
function Apply()
	doSubmit(0);
function doSubmit(del) {
	var f=document.frmSetup;
	var ip4=getCfg("LANIP").match(/\d{1,3}$/);
	if (!rangeCheck(f.dips,1,254,"IP
}")) return ;
	if (!rangeCheck(f.dipe,1,254,"IP
}")) return ;
	if (Number(f.dips.value)> Number(f.dipe.value)) {
	  alert("IP
}.");
	  return ;
	var net=getCfg("LANIP").replace(/\.\d{1,3}$/,".");
	setCfg("DHS",net+f.dips.value);
	setCfg("DHE",net+f.dipe.value);
	if (!del)
		if (chkStatic()) return ;
	for (i=0; i<max;i++)
		if (dhcpList[i])
			setCfg("LS"+(1+i),dhcpList[i]);
		else
			setCfg("LS"+(1+i),'');
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP',cPage);
function showStList()
var m='';
var r=1;
	for (i=0;i<dhcpList.length;i++) {
		var s=dhcpList[i].split(";");
		if (s.length<5) break;
		r++;
		m+='<tr>';
		m+='<td class=item_td>'+'<input type=checkbox  DISABLED '+((s[3]=='3')? '' : 'CHECKED')+'></td>';
		m+='<td class=item_td>'+s[0]+'</td>';
		m+='<td class=item_td>'+s[1]+'</td>';
		m+='<td class=item_td>'+s[2]+'</td>';
		m+='<td align=center class=item_td><img src=images/edit.png border=0 alt="
" onclick=doSel('+i+','+r+')>&nbsp;&nbsp;';
		m+='<img src=images/delet.png border=0 alt="
" onClick=doDel('+i+')></td>';
		m+='</tr>';
	s_edit_idx=dhcpList.length;
	document.write(m);
function showDynList()
var m='';
	for (i=0;i<dynList.length;i++) {
		var s=dynList[i].split(";");
		if (s.length<5) break;
		m+='<tr>';
		m+='<td class=item_td>'+s[0]+'</td>';
		m+='<td class=item_td>'+s[1]+'</td>';
		m+='<td class=item_td>'+s[2]+'</td>';
		m+='</tr>';
	document.write(m);
	document.getElementById('dsum').innerHTML=(dynList.length)+'/'+(dhe-dhs+1);
function cloneMAC(f){
	if (dynList.length==0) return ;
	dyn_sel_idx=f.dhcp.value;
	doEdit(dynList[dyn_sel_idx]);
function doSel(i,r){
	SelectRow("tab",r);
	doEdit(dhcpList[i]);
	s_edit_idx=i;
function doEdit(str){
	var f=document.frmSetup;
	var c = str.split(";");
	f.name.value = c[0];
	var mm = c[1].split(".")
	f.revIP4.value = mm[3];
	decomMAC2(f.mac,c[2],1);
	if (c[3]=='1') f.en[0].checked=true; else f.en[1].checked=true;
function doDel(i){
	if (!confirm("
?")) return;
	var f=document.frmSetup;
	rmEntry(dhcpList, i);
	doSubmit(1);
function chkStatic(){
	var f= document.frmSetup;
	var mac=combinMAC2(f.mac);
	var ip=f.revIP4.value;
	var name=f.name.value;
	if (ip=='' && name=='' && mac=='') return 0; // no input
	if (isBlank(name)) { alert("
IP-MAC
."); return -1; }
	if(!chksc(name,"
")) return -1;
	ip=Number(ip);
	if (!rangeCheck(f.revIP4,1,254,"IP
}")) return -1;
	if (!macsCheck(mac,"MAC 
}")) return -1;
	if (LANIP.match(/\d{1,3}$/)==parseInt(ip)) { alert("
."); return -1; }
	if (s_edit_idx >= s_max) { alert("
w "+s_max+" 
!"); return -1;}
	for (var k=0;k<dhcpList.length;k++) {
		if (s_edit_idx==k) continue;
		var ss=dhcpList[k].split(';');
		if (ss[1]==(netip+ip)) { alert("
}."); return -1; }
		if (ss[2]==mac) { alert("DHCP: 
"); return -1; }
	var set=name+';'+netip+ip+';'+mac+';'+(f.en[0].checked? '1' : '3')+';0';
	dhcpList[s_edit_idx]=set;
	return 0;
function genList(){
	var m='';
	for (i=0;i<dynList.length;i++) {
		var s=dynList[i].split(";");
		if (s.length<5) break;
		m+='<option value='+i+'>'+s[0]+','+s[1]+','+s[2]+'</option>';
	document.write(m);
function dhcp_apply()
	doSubmit(0);
