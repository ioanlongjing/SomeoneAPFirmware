function formatStr(num,len)
	var nbits = new String(num);
	var rst = "";
	a = len - (nbits.length)
	if ( a > 0)
	 for (i=0;i<a;i++)
		rst += "0";
	 rst += num;
	else
	 rst = num;
	return rst
function parseTime(t)
	var str=new String("");
	var tmp=parseInt(t);
	var sec=0,min=0,hr=0,day=0;
	sec=t % 60;  //sec
	min=parseInt(t/60) % 60; //min
	hr=parseInt(t/(60*60)) % 24; //hr
	day=parseInt(t/(60*60*24)); //day
	str=(day >0? day+" 
,  ":"0 
,  ")+(hr >0? formatStr(hr,2)+":":"00:")+(min >0? formatStr(min,2)+":":"00:")+(sec >0? formatStr(sec,2)+"":"00");
	return str;
