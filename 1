<script language="JavaScript">
var max=10;
var count=0;
var AppList=new Array();
var SelRow=-1;
var SelRowBC;
var prot=["tcp/udp","tcp","udp"];
var edit_flag = 0;//0:no 1:edit
function generateVS()
	var str=new String("");
	for(j=1;j<=max;j++){
		if(getCfg("SPL"+j))
   		{
		count++;
		AppList[j]=getCfg("SPL"+j);
		var s = AppList[j].split(";");
        str+="<tr>";
        str+="<td class=item_td><input type=\"checkbox\" name=\"en\""+(s[0]=="1"? " checked ": " ")+" disabled></td>";
        str+="<td class=item_td>"+s[1]+"</td>";
        str+="<td class=item_td>"+s[4]+"</td>";
        str+="<td class=item_td>"+s[2]+"</td>";
        str+="<td class=item_td>";
        str+="<img src=\"images/edit.png\" width=\"15\" height=\"17\" border=\"0\" alt=\"
\" onclick=\"EditRow("+j+")\">&nbsp;";
        str+="<img src=\"images/delet.png\" width=\"15\" height=\"18\" border=\"0\" alt=\"
\" onClick=\"doDelete("+(j+1)+")\">";
        str+="</td>";
        str+="</tr>";
    	}
	document.writeln(str);
	document.getElementById('sum').innerHTML="<span class=p_font>"+count+'/'+max+" </span>";
function EditRow(r)
	var f = document.frmSA;
	var s = AppList[r].split(";");
	f.editRow.value = r;
	if (s[0]=="1")
	    f.enable[0].checked=true;
	else
	    f.enable[1].checked=true;
	f.name.value = s[1];
	v=s[4].split("-");
	f.triPort1.value = v[0];
	f.triPort2.value = (v[1]?v[1]:"");
	f.pubPort.value = s[2];
	f.triType.selectedIndex=ProtToIdx(prot, s[5]);
	f.pubType.selectedIndex=ProtToIdx(prot, s[3]);
	SelectRow("tabSA",r);
	f.enable[0].focus();
	edit_flag  = 1;
function preSubmit()
var f = document.frmSA;
    for(j=1;j<=max;j++){
	if (AppList[j]) m=AppList[j];
	else m="";
	setCfg("SPL"+j,m);
    }
    form2Cfg(f);
    subForm(f,'do_cmd.htm','DHCP',cPage);
function Apply()
    if (checkParameter()==false) return;
    var f = document.frmSA;
    var num;
    
    if (f.editRow.value == "-1") num=count+1;
    else num=f.editRow.value;
    if (num>max)
    {
           alert("
w "+max+" 
!"); 
           return;
    }
    AppList[num]=boolToStr(f.enable[0].checked)+";"+
		f.name.value+";"+
		f.pubPort.value+";"+
		IdxToProt(prot,f.pubType.selectedIndex)+";"+
		portRange(f.triPort1.value,f.triPort2.value)+";"+
		IdxToProt(prot,f.triType.selectedIndex);
    preSubmit();
function doDelete(num)
    if (confirm("
?")==false) return;
    var f = document.frmVS;
    rmEntry(AppList, num-1);
    preSubmit();
function checkParameter()
	var f = document.frmSA;
	var i=0;
	//name
	if (isBlank(f.name.value)) { alert("
"); return false;}
	if(!chksc(f.name.value,"
")) return false;
	if(/.*[^\x00-\xff]+.*$/.test(f.name.value))
		alert("
J!");
		f.name.select();
		return false;
	// trigger port begin
	if (!chkPortRange(f.triPort1.value,f.triPort2.value,'
')) return false;
	// public port
	if (!chkPortRangeFmt(f.pubPort.value,"
: \n\n1.
p. 1024\t\n2.
p. 103,163,55,...\n3.
p. 103-157\n4.
p. 103,106-1333,154"))
		return false;
	if(!edit_flag)
		if (!checkSame())
			alert("
b.");
			return false;
	edit_flag = 0;
	if (f.enable[1].checked) return true; //skip chk with virtuals
	for (i=1;i<=max;i++)
		var sp=getCfg("VS"+i);
		var s=sp.split(";");
		if (s[0]=='1')
			if (!chkPortRange2(s[2],f.pubPort.value)){ alert("
"+ s [1] +"."); return false;}
	return true;
function checkSame()
    var f = document.frmSA;
    var str1=new String("");
    str1=f.pubPort.value+";"+IdxToProt(prot,f.pubType.selectedIndex)+";"+portRange(f.triPort1.value,f.triPort2.value)+";"+
             IdxToProt(prot,f.triType.selectedIndex);
    for(i=1;i<AppList.length;i++)
    	if ((i!=f.editRow.value) && AppList[i].toString().indexOf(str1) >=0)
    		return false;
    return true;
</script>
