function Exit()
	if (confirm("
X?")) self.close(); 
function trim(str){ 
     return str.replace(/(^\s*)|(\s*$)/g, "");
function Apply()
	document.getElementById("WscNewKey2").value = document.getElementById("WPAPSK1Temp").value;
	var f=document.frmDone;
	if(trim(f.ssid.value) == '')
		alert("
r!"); 
		f.ssid.focus();
		f.ssid.select();
		return ;
	if(f.WPAPSK1Temp.value.length < 8 || f.WPAPSK1Temp.value.length > 64)
		f.WPAPSK1Temp.focus();
		f.WPAPSK1Temp.select();
		alert("
J8~63
r!");
		return;
	f.AuthMode.value = "WPA2PSK;"+authmode_t.split(";")[1]+";"+authmode_t.split(";")[2];
	f.SecurityMode2.value = "3;"+securitmode_t.split(";")[1]+";"+securitmode_t.split(";")[2];
	f.EncrypType.value = "AES;"+encrytype_t.split(";")[1]+";"+encrytype_t.split(";")[2];
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP','total_status.html');
function check(obj){
  var test=obj.value.match(/[^A-Za-z0-9]/g);
  if(test!=null){
