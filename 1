<SCRIPT language=JavaScript>
var ctime="";
var max=10;
var dhcpList=new Array();
for (var i=0; i<max; i++)
	var l=getCfg("LS"+(1+i));
	if (l) dhcpList[i]=l;
	else break;
var dynList=new Array(
var s_max=32;
var dyn_sel_idx;
