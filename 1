function APClientEnableOnChange()
	document.getElementById("div_apcli_enable").style.visibility = "hidden";
	document.getElementById("div_apcli_enable").style.display = "none";
	document.getElementById("div_wds_add").style.visibility = "hidden";
	document.getElementById("div_wds_add").style.display = "none";
	document.getElementById("div_wds_list").style.visibility = "hidden";
	document.getElementById("div_wds_list").style.display = "none";
	document.getElementById("div_apcli_apply").style.visibility = "hidden";
	document.getElementById("div_apcli_apply").style.display = "none";
	document.getElementById("div_dis_all_apply").style.visibility = "visible";
	document.getElementById("div_dis_all_apply").style.display = "";
	if (document.wireless_apcli_setup.apcli_enable.options.selectedIndex == 1 || document.wireless_apcli_setup.apcli_enable.options.selectedIndex == 2)//1--->repeater 2--->apclient
		document.getElementById("div_apcli_enable").style.visibility = "visible";
		document.getElementById("div_apcli_enable").style.display = "";
		document.getElementById("div_apcli_apply").style.visibility = "visible";
		document.getElementById("div_apcli_apply").style.display = "";
		document.getElementById("div_dis_all_apply").style.visibility = "hidden";
		document.getElementById("div_dis_all_apply").style.display = "none";
	else if(document.wireless_apcli_setup.apcli_enable.options.selectedIndex == 3)//wds
		document.getElementById("div_wds_add").style.visibility = "visible";
		document.getElementById("div_wds_add").style.display = "";
		document.getElementById("div_wds_list").style.visibility = "visible";
		document.getElementById("div_wds_list").style.display = "";
		document.getElementById("div_dis_all_apply").style.visibility = "hidden";
		document.getElementById("div_dis_all_apply").style.display = "none";
function auth_sele_change()
	if(document.getElementById("encry__sele").options.selectedIndex == 0)
		document.wireless_apcli_setup.encry_type[0].checked = true;
		document.getElementById("encry_dis").disabled = false;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if(document.getElementById("encry__sele").options.selectedIndex == 1)
		document.wireless_apcli_setup.encry_type[1].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		document.getElementById("wep_key_5").style.visibility = "visible";
		document.getElementById("wep_key_5").style.display = "";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if(document.getElementById("encry__sele").options.selectedIndex == 2)
		document.wireless_apcli_setup.encry_type[1].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("wep_key_5").style.visibility = "visible";
		document.getElementById("wep_key_5").style.display = "";
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if((document.getElementById("encry__sele").options.selectedIndex == 3) )
		document.wireless_apcli_setup.encry_type[3].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
	else if( (document.getElementById("encry__sele").options.selectedIndex == 4) )
		document.wireless_apcli_setup.encry_type[4].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = false;
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
	else
		document.wireless_apcli_setup.encry_type[5].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = false;
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
function auth_sele_init_change()
	if(document.getElementById("encry__sele").options.selectedIndex == 0)
		document.getElementById("encry_dis").disabled = false;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if(document.getElementById("encry__sele").options.selectedIndex == 1)
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		document.getElementById("wep_key_5").style.visibility = "visible";
		document.getElementById("wep_key_5").style.display = "";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if(document.getElementById("encry__sele").options.selectedIndex == 2)
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("wep_key_5").style.visibility = "visible";
		document.getElementById("wep_key_5").style.display = "";
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if((document.getElementById("encry__sele").options.selectedIndex == 3) )
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
	else if( (document.getElementById("encry__sele").options.selectedIndex == 4) || (document.getElementById("encry__sele").options.selectedIndex == 5))
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = false;
		document.getElementById("wep_key_5").style.visibility = "hidden";
		document.getElementById("wep_key_5").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
function encryp_dis()
	//alert("dis");
	document.getElementById("wep_key_5").style.visibility = "hidden";
	document.getElementById("wep_key_5").style.display = "none";
	document.getElementById("div_wpa").style.visibility = "hidden";
	document.getElementById("div_wpa").style.display = "none";
	document.getElementById("div_wep").style.visibility = "hidden";
	document.getElementById("div_wep").style.display = "none";
function encryp_wep64()
	document.getElementById("wep_key_5").style.visibility = "visible";
	document.getElementById("wep_key_5").style.display = "";
	document.getElementById("div_wpa").style.visibility = "hidden";
	document.getElementById("div_wpa").style.display = "none";
	document.getElementById("div_wep").style.visibility = "visible";
	document.getElementById("div_wep").style.display = "";
	if(document.wireless_apcli_setup.wep_key_meth[0].checked == true)//ASCII
		document.getElementById("wep5_key1").maxLength = 5;
		document.getElementById("wep5_key2").maxLength = 5;
		document.getElementById("wep5_key3").maxLength = 5;
		document.getElementById("wep5_key4").maxLength = 5;
		document.getElementById("wep5_key1").size = 5;
		document.getElementById("wep5_key2").size = 5;
		document.getElementById("wep5_key3").size = 5;
		document.getElementById("wep5_key4").size = 5;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
	else//HEX
		document.getElementById("wep5_key1").maxLength = 10;
		document.getElementById("wep5_key2").maxLength = 10;
		document.getElementById("wep5_key3").maxLength = 10;
		document.getElementById("wep5_key4").maxLength = 10;
		document.getElementById("wep5_key1").size = 10;
		document.getElementById("wep5_key2").size = 10;
		document.getElementById("wep5_key3").size = 10;
		document.getElementById("wep5_key4").size = 10;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
	return;
function encryp_wep128()
	document.getElementById("wep_key_5").style.visibility = "visible";
	document.getElementById("wep_key_5").style.display = "";
	document.getElementById("div_wpa").style.visibility = "hidden";
	document.getElementById("div_wpa").style.display = "none";
	document.getElementById("div_wep").style.visibility = "visible";
	document.getElementById("div_wep").style.display = "";
	if(document.wireless_apcli_setup.wep_key_meth[0].checked == true)//ASCII
		document.getElementById("wep5_key1").maxLength = 13;
		document.getElementById("wep5_key2").maxLength = 13;
		document.getElementById("wep5_key3").maxLength = 13;
		document.getElementById("wep5_key4").maxLength = 13;
		document.getElementById("wep5_key1").size = 13;
		document.getElementById("wep5_key2").size = 13;
		document.getElementById("wep5_key3").size = 13;
		document.getElementById("wep5_key4").size = 13;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
	else//HEX
		document.getElementById("wep5_key1").maxLength = 26;
		document.getElementById("wep5_key2").maxLength = 26;
		document.getElementById("wep5_key3").maxLength = 26;
		document.getElementById("wep5_key4").maxLength = 26;
		document.getElementById("wep5_key1").size = 26;
		document.getElementById("wep5_key2").size = 26;
		document.getElementById("wep5_key3").size = 26;
		document.getElementById("wep5_key4").size = 26;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
