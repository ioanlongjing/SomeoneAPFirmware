<script>
var wsc_enable = "";
var wsc_function_enable = "";
var wsc_pin_code_s = "";
var wsc_device_pin = "";
var wsc_mod = "";
var wsc_configuredStatus = "";
var WscPinMode = "";
function init()
	if(wsc_enable == 0)
		document.wireless_simple_config_setup.wpsfun[1].checked = true;
		document.wireless_simple_config_setup.wpsmod[0].disabled = true;
		document.wireless_simple_config_setup.wpsmod[1].disabled = true;
		document.wireless_simple_config_setup.wsc_pin_code_w.disabled = true;
	else if(wsc_enable == 1)
		document.wireless_simple_config_setup.wpsfun[0].checked = true;
		document.wireless_simple_config_setup.wpsmod[0].disabled = false;
		document.wireless_simple_config_setup.wpsmod[1].disabled = false;
		document.wireless_simple_config_setup.wsc_pin_code_w.disabled = false;
		if(wsc_pin_code_s == wsc_device_pin)
			document.wireless_simple_config_setup.wsc_pin_code_w.value = "";
		else
			document.wireless_simple_config_setup.wsc_pin_code_w.value = wsc_pin_code_s;
	if(wsc_mod == 1)
		document.wireless_simple_config_setup.wpsmod[0].checked = true;
	else if(wsc_mod == 2)
		document.wireless_simple_config_setup.wpsmod[1].checked = true;
	changeSel();
function wps_enable()
	document.wireless_simple_config_setup.WscEnable2.value = 1;	
	document.wireless_simple_config_setup.WscMode2.value=wsc_mod;
	document.wireless_simple_config_setup.WscPinMode.value=Number(WscPinMode);
	var f=document.wireless_simple_config_setup;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SIMPLE_CONFIG_SETUP',cPage);
	return true;
function wps_disable()
	document.wireless_simple_config_setup.WscEnable2.value = 0;	
	document.wireless_simple_config_setup.WscMode2.value=wsc_mod;
	document.wireless_simple_config_setup.WscPinMode.value=Number(WscPinMode);
	var f=document.wireless_simple_config_setup;	
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SIMPLE_CONFIG_SETUP',cPage);
	return true;
function Apply()
	var wsc_pin_code, accum;
	if(document.wireless_simple_config_setup.wpsmod[0].checked == true)
		document.wireless_simple_config_setup.WscMode2.value = 1;//pin
		if(document.wireless_simple_config_setup.wpsmod[0].checked)
			if(document.wireless_simple_config_setup.Pinmode[1].checked)
			{
				if (document.wireless_simple_config_setup.wsc_pin_code_w.value.length != 8)
				{
					alert("
					window.location.reload();
					return false;
				}
				else
				{
					wsc_pin_code = document.wireless_simple_config_setup.wsc_pin_code_w.value;
					wsc_pin_code = 1*wsc_pin_code;
					accum = 0;
					accum += 3 * (parseInt(wsc_pin_code / 10000000) % 10);
					accum += 1 * (parseInt(wsc_pin_code / 1000000) % 10);
					accum += 3 * (parseInt(wsc_pin_code / 100000) % 10);
					accum += 1 * (parseInt(wsc_pin_code / 10000) % 10);
					accum += 3 * (parseInt(wsc_pin_code / 1000) % 10);
					accum += 1 * (parseInt(wsc_pin_code / 100) % 10);
					accum += 3 * (parseInt(wsc_pin_code / 10) % 10);
					accum += 1 * (parseInt(wsc_pin_code / 1) % 10);
					
					if ((accum % 10) != 0)
					{
						alert("
~");
						return false;
					}
				}			
				document.wireless_simple_config_setup.WscPinMode.value=1; 
			}
			else
			{
				document.wireless_simple_config_setup.wsc_pin_code_w.value = wsc_pin_code_s;
				document.wireless_simple_config_setup.WscPinMode.value=0;
			}
	else
		document.wireless_simple_config_setup.WscMode2.value = 2;//pbc
		document.wireless_simple_config_setup.WscPinMode.value=Number(WscPinMode);
	var f=document.wireless_simple_config_setup;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SIMPLE_CONFIG_DEVICE',cPage);
	return true;
function choosePIN()
	if(document.wireless_simple_config_setup.Pinmode[0].checked)
		document.wireless_simple_config_setup.wsc_pin_code_w.value ="";
		document.wireless_simple_config_setup.wsc_pin_code_w.disabled =true;			
	else
		document.wireless_simple_config_setup.wsc_pin_code_w.disabled =false;
function changeSel()
	if(WscPinMode == 0)
		document.wireless_simple_config_setup.Pinmode[0].checked = true;
	else if(WscPinMode == 1)
		document.wireless_simple_config_setup.Pinmode[1].checked = true;
	if(document.wireless_simple_config_setup.wpsmod[1].checked==true||wsc_enable == 0)
		document.getElementById("Pinblock").style.display ="none";
	else if(document.wireless_simple_config_setup.wpsmod[0].checked==true)
		document.getElementById("Pinblock").style.display ="";
	else
		document.getElementById("Pinblock").style.display ="none";
	choosePIN();
</script>
