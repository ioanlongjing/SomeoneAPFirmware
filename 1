","(1,Lower)","(2,Lower)","(3,Lower)","(4,Lower)","(5,Lower)","(5,Upper)","(6,Lower)","(6,Upper)","(7,Lower)","(7,Upper)","(8,Lower)","(8,Upper)","(9,Lower)","(9,Upper)","(10,Upper)","(11,Upper)","(12,Upper)","(13,Upper)"];
		//index 18
		var str1 = "";
		var channel_tm = 0;
		PhyMode = 1 * PhyMode;
		channel_in = 1 * channel_in;
		ht_extcha = 1 * ht_extcha;
		if(PhyMode == 0 || PhyMode == 1 || PhyMode == 4)
			//alert("debug channel 2");
			str1 = channel_g[channel_in];
		else if(PhyMode == 9)
			//alert("debug channel 1");
			if(channel_in < 5)
				channel_tm = channel_in;
			else if(channel_in < 10)	
				channel_tm = channel_in + (1 - ht_extcha) + (channel_in - 5);
			else if(channel_in < 14)
				channel_tm = channel_in + 5;
				
			str1 = channel_n[channel_tm];
		document.write(str1);
	  </script>
