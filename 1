		f.auto_dns.checked = true;
		EnableManuDns();
function Apply() {
	var	f=document.DHC;
	if (!verifyMAC(f.WMAC,"
}",1)) return	;
	if(f.auto_dns.checked==false)
		if (!verifyIP0(f.DS1,"
nDNS")) return ;
		if(f.DS2.value != '')
				if (!verifyIP0(f.DS2,"
nDNS")) return ;
	if (f.DMTU.value.length) if (!rangeCheck(f.DMTU,576,1500,"MTU")) return ;
	setCfg("DSFIX",evalDnsFix(f.DS1.value,f.DS2.value));
	setCfg("WANT",2);
	if(f.auto_dns.checked == true)
		setCfg("AUTODNS",1);
	else
		setCfg("AUTODNS",0);
	form2Cfg(f);
	if (getCfg("WMAC")=='')	setCfg("clnEn",0); else	setCfg("clnEn",1);
	subForm(f,'do_cmd.htm','DHCP',cPage);
function EnableManuDns()
		if(document.DHC.auto_dns.checked==false)
			document.DHC.DS1.disabled = true;
			document.DHC.DS2.disabled = true;
		else
			document.DHC.DS1.disabled = false;
			document.DHC.DS2.disabled = false;
</SCRIPT>
