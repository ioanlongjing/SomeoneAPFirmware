function trim(str){ //?
return str.replace(/(^\s*)|(\s*$)/g, ""); 
function initSIP(){
	cfg2Form(document.SIP);
function chkSIP() {
	var f=document.SIP;
	var ip,gw;
	ip = trim(f.WANIP.value);
	gw = trim(f.WANGW.value);
	if(!(ip.split(".")[0] == gw.split(".")[0] && ip.split(".")[1] == gw.split(".")[1] && ip.split(".")[2] == gw.split(".")[2]))
		alert("IP
J!");
		return;
	if (!verifyIP2(f.WANIP,"
}")) return ;
	if (!ipMskChk(f.WANMSK,"
n")) return ;
	if (!verifyIP2(f.WANGW,"
D")) return ;
	if (!verifyIP2(f.DS1,"
nDNS")) return ;
	if (!verifyIP0(f.DS2,"
nDNS")) return ;
	//if(f.DS2.value == '')
	//	f.DS2.value = "168.95.192.1";
	setCfg("DSFIX",1);
	form2Cfg(f);
	switchPage('C1','D');
//////////// PPPOE
function initPOE() {
	cfg2Form(document.POE);
function chkPOE() {
	var f=document.POE;
	if (!chkStrLen(f.PUN,0,255,"
")) return ;
	if(trim(f.PPW.value)=="")
		alert("
X!");
		return;
	if(trim(f._ps2.value)=="")
		alert("
X!");
		return;
	var str1 = f.PPW.value;
	var str2 = f._ps2.value;
	if(!(str1.indexOf(str2)==0 && str2.indexOf(str1)==0))
		alert("
P!!!") ;
		return;
	if (isBlank(f.PUN.value)) { alert("
"); return ;}
	form2Cfg(f);
	switchPage('C3','D');
////////// PPTP 
addCfg("PTPW",  , "******" );
function initPTP(){
	cfg2Form(document.PTP);
function chkPTP() {
	var f=document.PTP;
	if (!verifyIP2(f.PTSVR,"Server IP Address")) return ;
	if (!verifyIP2(f.PTIP,"IP Address")) return ;
	if (!ipMskChk(f.PTMSK,"Subnet Mask")) return ; 
	if (!chkStrLen(f.PTUN,0,255,"User Name")) return ;
	if (!chkStrLen(f.PTPW,0,255,"User Password")) return ;
	if (!chkPwdUpdate(f.PTPW,f._ps2,f._changed)) return;
	setCfg("PTWANIF", 1);
	form2Cfg(f);
	switchPage('C4','D');
