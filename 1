function trim(str){ //?
return str.replace(/(^\s*)|(\s*$)/g, ""); 
var pppoe_pass = "";
var option_mode = "";
var autodns_mode = "";
autodns_mode = 1 * autodns_mode;
function Apply() {
	var f=document.POE;
	if (!rangeCheck(f.PMTU,1000,1492,"MTU")) return ;
	if (!rangeCheck(f.PIDL,0,60,"
")) return ;
	if (isBlank(f.PUN.value)) { alert("
"); return ;}
	if (!strCheck(f.PUN,"
")) return;
	if(/.*[^\x00-\xff]+.*$/.test(f.PUN.value))
		alert("
J!");
		f.PUN.select();
		return;
	//alert("1");
	//if (!strCheck(f.PPW,"
X")) return;
	//alert("2");
	//if (!chkPwdUpdate(f.PPW,f._ps2,f._changed)) return ;
	if(isBlank(f.PPW.value)){ alert("
X!"); return ;}
	if(isBlank(f._ps2.value)){ alert("
X!"); return ;}
	if(/.*[^\x00-\xff]+.*$/.test(f.PPW.value))
		alert("
J!");
		f.PPW.select();
		return;
	var str1 = f.PPW.value;
	var str2 = f._ps2.value;
	if(!(str1.indexOf(str2)==0 && str2.indexOf(str1)==0))
		alert("
P!!!") ;
		return;
	if (f.PIPEN[1].checked)
		if (!verifyIP2(f.PIP,"
}")) return ;
	if (f.auto_dns.checked==false)
		if (!verifyIP0(f.DS1,"
nDNS")) return ;
		if(f.DS2.value != '')
			if (!verifyIP0(f.DS2,"
nDNS")) return ;
	//if(f.DS1.value == '')
	//	f.DS1.value = "168.95.192.1";
	if (!verifyMAC(f.WMAC,"
}",1)) return ;
	if(f.auto_dns.checked == true)
		setCfg("AUTODNS",1);
	else
		setCfg("AUTODNS",0);
	setCfg("DSFIX",evalDnsFix(f.DS1.value,f.DS2.value));
	if (f.PIPEN[0].checked) setCfg("DSFIX",0); // if dynamic, don't use fix dns
	setCfg("WANT",3);
	form2Cfg(f);
	setCfg("PIDL",getCfg("PIDL")*60);
	subForm(f,'do_cmd.htm','DHCP',cPage);
var advance_btn_click=0;
function pppoe_advance()
	if(!advance_btn_click)
		document.getElementById("pppoe_adv_info").style.visibility = "visible";
		document.getElementById("pppoe_adv_info").style.display = "";
		document.getElementById("pppoe_adv").innerHTML = "-
		advance_btn_click = 1;
	else
		document.getElementById("pppoe_adv_info").style.visibility = "hidden";
		document.getElementById("pppoe_adv_info").style.display = "none";
		document.getElementById("pppoe_adv").innerHTML = "+
		advance_btn_click = 0;
		EnableManuDns();
	return;
function init(){
	var f=document.POE;
	cfg2Form(f);
	if (f.clnEn.value=='1') setCln(f,getCfg("WMAC")); else clrCln(f);
	clickPOE();
	f.PPW.value = pppoe_pass;
	f._ps2.value = "";
	document.getElementById("pppoe_adv_info").style.visibility = "hidden";
	document.getElementById("pppoe_adv_info").style.display = "none";
	document.getElementById("pppoe_service_div").style.visibility = "hidden";
	document.getElementById("pppoe_service_div").style.display = "none";
	document.getElementById("pppoe_ip_div").style.visibility = "hidden";
	document.getElementById("pppoe_ip_div").style.display = "none";
	document.getElementById("pppoe_max_time_div").style.visibility = "hidden";
	document.getElementById("pppoe_max_time_div").style.display = "none";
	if(autodns_mode == 1)
		f.auto_dns.checked = true;
function clickPOE()
	var f = document.POE; 
	var b = ((f.PIPEN[0].checked)? true : false);
	f.PIP.disabled = f.DS1.disabled = f.DS2.disabled = b; 
function EnableManuDns()
		if(document.POE.auto_dns.checked==false)
			document.POE.DS1.disabled = true;
			document.POE.DS2.disabled = true;
		else
			document.POE.DS1.disabled = false;
			document.POE.DS2.disabled = false;
