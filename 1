var ddns_status = ;
var totalCountdownTime = 30;
function reFresh(TT)
	if(TT > 0)
		//document.all.vCountdown.innerHTML=TT;
		TT = TT - 1 ;
		setTimeout("reFresh("+TT+");","6000");
	else
		ddns_refresh();
function SelectService(f)
	var text = '';
	var text2 = '';
	var i;
	var ODL;
	var dl = getCfg("ddns1");
	if (((f.serverName.value=="dyndns")&&(decomList(dl,9,1,";")=="dyndns"))||
		((f.serverName.value=="noip")&&(decomList(dl,9,1,";")=="noip"))||
		((f.serverName.value=="qdns")&&(decomList(dl,9,1,";")=="qdns"))||
		((f.serverName.value=="changeip")&&(decomList(dl,9,1,";")=="changeip"))||
		((f.serverName.value=="dtdns")&&(decomList(dl,9,1,";")=="dtdns"))
	{			
		if (getCfg("ddnsEnable")==1)
			text += '<table cellpadding=0 width=70% class=content_table>';
			text += '<tr><td colspan=2 class=web_title2>DDNS
A</td></tr>';
			if (ddns_status == 0)
			{
				if (f.ddnsEnable[0].checked)
				{
					text += '<tr><td width=27% height=20 class=itemdesc_td>
A</td><td align=left class=item_td>
u</td></tr>';
					reFresh(totalCountdownTime);
				}			
			}
			else
			{
				if (f.ddnsEnable[0].checked)
				{
					text += '<tr><td width=27% height=20 valign=top class=itemdesc_td>
A</td><td align=left class=item_td>
T.</td></tr>';
				}
			}
			text += '</td></tr>';
			text += '<tr><td width=27% height=20></td><td align=left></td></tr>';
			text += '</table>';
	setIdVal("tServiceInfo",text);
	resetValue(f);
