	init();
function Apply() {
	var f=document.frmSetup;
	if (isBlank(f.hostName.value)) { alert("
!"); return;}
	if (isBlank(f.userName.value)) { alert("
!"); return;}
	if (isBlank(f.passWord.value)) { alert("
!"); return;}
	if (!chken(f.passWord.value,"
X")) return;
	if(/.*[^\x00-\xff]+.*$/.test(f.hostName.value))
		alert("
J!");
		f.hostName.select();
		return ;
	if(/.*[^\x00-\xff]+.*$/.test(f.userName.value))
		alert("
J!");
		f.userName.select();
		return ;
	if(/.*[^\x00-\xff]+.*$/.test(f.passWord.value))
		alert("
J!");
		f.passWord.select();
		return ;
	setCfg("ddns1", "1;"+
				f.serverName.value+";"+
				f.hostName.value+";"+
				f.userName.value+";"+
				f.passWord.value+";"+
				";"+";"+";");
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DDNS',cPage);
