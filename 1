//[index,ntp_flag,up_time,ntp_time,str_time,message]
var syslogList=new Array(
var dataLists = new Array();
function init(f) {
	var i,tt;
	var syslog_len;
	syslog_len = syslogList.length/6;
	for(i=0;i<syslog_len;i++)
		tt = dateStr(Number(syslogList[(syslog_len-1-i)*6+3]));
		dataLists[i] =new Array(tt,syslogList[(syslog_len-1-i)*6+5]);
	showSysLog();
	cfg2Form(f);
function showSysLog()
   var str=new String("");
   var f=document.frmLog;
   var p=parseInt(f.curpage.value);
   var max=(dataLists.length%20==0? dataLists.length/20:parseInt(dataLists.length/20)+1);
   	
   	/* show 20 pages most.*/
   if (max>20)
   	   max=20;
   if (max==0 || max==1)
   {
      f.Pp1.disabled=true;
      f.Np1.disabled=true;
   }
   else
   {
    if (p==1) 
    {
      f.Pp1.disabled=true;
      f.Np1.disabled=false;    
    }
    if (p==max)
    {
      f.Pp1.disabled=false;
      f.Np1.disabled=true;
    }
    if (p > 1 && p < max)
    {
      f.Pp1.disabled=false;
      f.Np1.disabled=false;
    }
   }
   if (document.layers) return true; 
   {
        str+="<table width=100% class=content_table style=\"{font-family:arial;font-size:12px;color:#000000;}\">";
        str+="<tr>"
        str+="<td colspan=2 align=left> "+p+"/"+(max==0? 1:max)+"
</b></td>";
        str+="</tr>";
        str+="<tr>"
        str+="<td width=25% align=left style=\"COLOR: black;background-color:#EAEAEA;TEXT-ALIGN: left;\">
</td>";
        str+="<td width=75% align=left style=\"COLOR: black;background-color:#EAEAEA;TEXT-ALIGN: left;\">
</td>";
        str+="</tr>";
        for (var i=((p-1)*20);i < p*20;i++)
        {
            if (i>=dataLists.length) break;
       
            str+="<tr>";
            str+="<td width=22% class=item_td>"+dataLists[i][0]+"</td>";
            str+="<td width=78% class=item_td>"+dataLists[i][1]+"</td>";
            str+="</tr>";
        }
        str+="</table>";
   }
   if (document.all)
   {
      document.all("sLog").innerHTML=str;
   }
   else if (document.getElementById)
   {
        document.getElementById("sLog").innerHTML=str;
   }
function ToPage(p)
   if (dataLists.length==0) return;
   var f=document.frmLog;
   var maxPage=(dataLists.length%20==0? dataLists.length/20:parseInt(dataLists.length/20)+1)
   switch (p)
   {
   case "0":
        f.curpage.value=maxPage;
        break;
   case "1":
        f.curpage.value=1;
        break;
   case "-1":
        f.curpage.value=(parseInt(f.curpage.value)-1 <=0? 1:parseInt(f.curpage.value)-1);
        break;
   case "+1":
        f.curpage.value=(parseInt(f.curpage.value)+1 >=maxPage? maxPage:parseInt(f.curpage.value)+1);
        break;
   }
   showSysLog();
function preDownload(f) {
   f.action="syslog.bin" ;
   f.submit() ;
function preDownload_s(f) {
   f.action="securelog.bin" ;
   f.submit() ;
function preClear(f) {
   f.action="do_cmd.htm" ;
   f.submit() ;
function apply2(){
	//alert("this apply!");
	var f=document.frmLog;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP',cPage);
	return true;
