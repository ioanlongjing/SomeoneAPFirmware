<script language="JavaScript">
var max=10;
var lanip=getCfg("LANIP");
var lanmsk=getCfg("LANMSK");
var netip=lanip.replace(/\.\d{1,3}$/,".");
var VSList=new Array();
var SelRowBC;
var SelRow = -1; 
var count=0;
var prot=["tcp/udp","tcp","udp"];
var edit_flag = 0;//0:no 1:edit
function generateHTML()
	var str=new String("");
	var n=0;
	for(j=1;j<=max;j++){
		if(getCfg("VS"+j))
			count++;
			VSList[j]=getCfg("VS"+j);
			var s = VSList[j].split(";");
			var t = s[2].split("-");
			var temp=t[1]-t[0]-(-s[5]);
			str+="<tr>";
			str+="<td class=item_td><input type=\"checkbox\" name=\"en\""+(s[0]=="1"? " checked " : " ")+" disabled></td>";
			str+="<td class=item_td>"+s[1]+"</td><td class=item_td>"+s[4]+"&nbsp;&nbsp;</td>";
			//str+="<td class=item_td>"+ProtToStr(s[3])+" "+s[5]+"-"+temp+"/"+s[2]+"</td>";
			if(s[3] == "tcp/udp")
				str+="<td class=item_td>TCP+UDP</td>";
			else
				str+="<td class=item_td>"+s[3]+"</td>";
				
			if(s[2].split("-")[0] == s[2].split("-")[1])
				str+="<td class=item_td>"+s[2].split("-")[0]+"</td>";
			else
				str+="<td class=item_td>"+s[2]+"</td>";
				
			str+="<td class=item_td>"+s[5]+"</td>";
			//str+="<td class=item_td>"+showSchedule(s[6],s[7],s[8])+"</td>";
			str+="<td class=item_td>";
			str+="<img src=\"./images/edit.png\" width=\"15\" height=\"17\" border=\"0\" alt=\"
\" onclick=\"EditRow("+j+")\">&nbsp;";
			str+="<img src=\"./images/delet.png\" width=\"15\" height=\"18\" border=\"0\" alt=\"
\" onClick=\"doDelete("+(j+1)+")\">";
			str+="</td>";
			str+="</tr>";
			n++;
	document.writeln(str);
	document.getElementById('sum').innerHTML="<span class=p_font>"+n+'/'+max+" </span>";
function EditRow(r)
	var f = document.frmVS;
	var s = VSList[r].split(";");
	var pri,pub;
	f.editRow.value=r;
	if (s[0]=="1")
	    f.enable[0].checked=true;
	else
	    f.enable[1].checked=true;
	f.name.value = s[1];
	f.ip.value = s[4];
	f.protocol.selectedIndex=ProtToIdx(prot,s[3]);
	pub=s[2].split("-");
	f.priPort_s.value = s[5];
	f.priPort_e.value = pub[1]-pub[0]-(-s[5]);;
	f.pubPort_s.value = pub[0];
	f.pubPort_e.value = pub[1];	
    	setSchedule(f,s[6],s[7],s[8]);
	SelectRow("tabVS",r);
	f.enable[0].focus();
	edit_flag = 1;
function Cancel()
    var f = document.frmVS;
    f.editRow.value = "-1"
    SelectRow("tabVS",-1)
    f.reset();
function preSubmit()
    var f = document.frmVS;
    for(j=1;j<=max;j++){
	if (VSList[j]) m=VSList[j];
	else m="";
	setCfg("VS"+j,m);
    }
    form2Cfg(f);
    subForm(f,'do_cmd.htm','DHCP',cPage);
function Apply()
    if (checkParameter()==false) return;
    var f = document.frmVS;
    var num;
    
    num=parseInt(f.editRow.value)+1;
    if (f.editRow.value == "-1") num=count+1;
    else  num=f.editRow.value;
    
    if (num>max)
    {
           alert("
!"); 
           return;
    }
	if(edit_flag === 0)
		for(i=0;i < count ;i++)
			var s = VSList[i+1].split(";");
			var str1 = f.name.value;
			var str2 = s[1];
			var protocol_v ;//1 tcp,2 udp,3 tcp+udp
			var pubport,priport_s,priport_e;
			var ipv = 0;
			if(str1.indexOf(str2)==0 && str2.indexOf(str1)==0)
			{
				alert("
b.");
				return;
			}
			if(s[3] == "tcp/udp")
				protocol_v = 0;
			else if(s[3] == "tcp")
				protocol_v = 1;
			else
				protocol_v = 2;
			str1 = s[4];
			str2 = f.ip.value;
			if(str1.indexOf(str2)==0 && str2.indexOf(str1)==0)
			{
				ipv = 1;
			}
			pub=s[2].split("-");
			pubport = parseInt(s[5]);
			priport_s = parseInt(pub[0]);
			priport_e = parseInt(pub[1]);
			if(ipv &&(protocol_v == f.protocol.selectedIndex )&&(pubport == f.priPort_s.value) && (priport_s == f.pubPort_s.value)&&(priport_e == f.pubPort_e.value))
			{
				alert("
b.");
				return;
			}
	edit_flag = 0;
    VSList[num]=boolToStr(f.enable[0].checked)+";"+
                          f.name.value+";"+
                          f.pubPort_s.value+"-"+f.pubPort_e.value+";"+
                          IdxToProt(prot,f.protocol.selectedIndex)+";"+
                          evalIP(f.ip.value)+";"+
                          f.priPort_s.value+";"+ 
                          boolToStr(f.schd[1].checked)+";"+ 
                          Schedule2str(f);
    preSubmit();
function doDelete(num)
    if (confirm("
?")==false) return;
    var f = document.frmVS;
    rmEntry(VSList, num-1);
    preSubmit();
function checkParameter()
   var f = document.frmVS;
   var i=0;
   var range=0;   
   //name
   var n=f.name.value; 
   	if (isBlank(n)) { alert("
"); return false;}
    if(!chksc(n,"
")) return false;
    if(/.*[^\x00-\xff]+.*$/.test(n))
		alert("
J!");
		f.name.select();
		return false;
	// private ip
	if (!validIP(f.ip.value)) { alert("
}"); return false;}
	if (evalIP(f.ip.value)==lanip) { alert("IP
IP."); return false;}
	if (!isSameClass(f.ip.value,lanip,lanmsk)) { alert("
}."); return false; }
	// public port
	if (!isNumber(f.pubPort_s.value)) { alert("
"); return false;}
	if (!validPort(f.pubPort_s.value)) return false;
	if(f.pubPort_e.value == '') f.pubPort_e.value = f.pubPort_s.value;
	if (!isNumber(f.pubPort_e.value)) { alert("
"); return false;}
	if (!validPort(f.pubPort_e.value)) return false;
	if(f.pubPort_e.value-f.pubPort_s.value<0) { alert("
."); return false; }
	range=f.pubPort_e.value-f.pubPort_s.value;
	f.priPort_e.value=0;
	f.priPort_e.value=f.priPort_s.value-(-range);
	// private port
	if (!isNumber(f.priPort_s.value)) { alert("
"); return false; }
	if (!validPort(f.priPort_s.value)) return false;
	//if (!isNumber(f.priPort_e.value)) { alert("
"); return false; }
	//if (!validPort(f.priPort_e.value)) return false;
	if(f.schd[1].checked){
    	st=(f.hour1.selectedIndex+12*f.am1.selectedIndex)*3600+(f.min1.selectedIndex)*5*60;
    	et=(f.hour2.selectedIndex+12*f.am2.selectedIndex)*3600+(f.min2.selectedIndex)*5*60;
    	if(et<st || (f.day1.selectedIndex==(f.day2.selectedIndex+1))){ alert("
"); return false;} 
    }
	if (f.enable[1].checked) return true; // don't chk anymore
	// check the same public port
	for(i=1;i<VSList.length;i++)
		if (Number(f.editRow.value)==i) continue;
		s=VSList[i].split(";");
		if (s[0]!="1") continue; // if disable, skip
		if((f.protocol.selectedIndex==ProtToIdx(prot,s[3]))||(ProtToIdx(prot,s[3])==0)||(f.protocol.selectedIndex==0))
			if((Number(f.pubPort.value)==Number(s[2]))) { alert("
C"); return false;}
			if(evalIP(f.ip.value)==s[4]&&(Number(f.priPort.value)==Number(s[5]))) { alert("
C"); return false;}
	for (i=1;i<=max;i++)
		var sp=getCfg("SPL"+i);
		var s=sp.split(";");
		if (s[0]=='1')
			if (!chkPortRange2(f.pubPort.value,s[2])) { alert("
"+s[1]+"
C"); return false;}
	}*/
	return true;
</script>
