function encryp_tkip_aes_mixed()
	//alert("dis");
	document.getElementById("wep_key_5").style.visibility = "hidden";
	document.getElementById("wep_key_5").style.display = "none";
	document.getElementById("div_wpa").style.visibility = "visible";
	document.getElementById("div_wpa").style.display = "";
	document.getElementById("div_wep").style.visibility = "hidden";
	document.getElementById("div_wep").style.display = "none";
function APClientWepKeyLengthOnChange()
	document.wireless_apcli_setup.apcli_wep_key_1.value = '';
	document.wireless_apcli_setup.apcli_wep_key_2.value = '';
	document.wireless_apcli_setup.apcli_wep_key_3.value = '';
	document.wireless_apcli_setup.apcli_wep_key_4.value = '';
	if (document.wireless_apcli_setup.apcli_wep_key_length.options.selectedIndex == 0)
		// <!-- KEY length 64 bits -->
		if (document.wireless_apcli_setup.apcli_wep_key_method.options.selectedIndex == 0)
			// <!-- HEX -->
			document.wireless_apcli_setup.apcli_wep_key_1.maxLength = 10;
			document.wireless_apcli_setup.apcli_wep_key_2.maxLength = 10;
			document.wireless_apcli_setup.apcli_wep_key_3.maxLength = 10;
			document.wireless_apcli_setup.apcli_wep_key_4.maxLength = 10;
		else
			// <!-- ASCII -->
			document.wireless_apcli_setup.apcli_wep_key_1.maxLength = 5;
			document.wireless_apcli_setup.apcli_wep_key_2.maxLength = 5;
			document.wireless_apcli_setup.apcli_wep_key_3.maxLength = 5;
			document.wireless_apcli_setup.apcli_wep_key_4.maxLength = 5;
	else
		// <!-- KEY length 128 bits -->
		if (document.wireless_apcli_setup.apcli_wep_key_method.options.selectedIndex == 0)
			// <!-- HEX -->
			document.wireless_apcli_setup.apcli_wep_key_1.maxLength = 26;
			document.wireless_apcli_setup.apcli_wep_key_2.maxLength = 26;
			document.wireless_apcli_setup.apcli_wep_key_3.maxLength = 26;
			document.wireless_apcli_setup.apcli_wep_key_4.maxLength = 26;
		else
			// <!-- ASCII -->
			document.wireless_apcli_setup.apcli_wep_key_1.maxLength = 13;
			document.wireless_apcli_setup.apcli_wep_key_2.maxLength = 13;
			document.wireless_apcli_setup.apcli_wep_key_3.maxLength = 13;
			document.wireless_apcli_setup.apcli_wep_key_4.maxLength = 13;
function init_all()
	document.getElementById("div_apcli_enable").style.visibility = "hidden";
	document.getElementById("div_apcli_enable").style.display = "none";
	document.getElementById("div_wds_add").style.visibility = "hidden";
	document.getElementById("div_wds_add").style.display = "none";
	document.getElementById("div_apcli_apply").style.visibility = "hidden";
	document.getElementById("div_apcli_apply").style.display = "none";
	document.getElementById("div_wds_list").style.visibility = "hidden";
	document.getElementById("div_wds_list").style.display = "none";
	if(1 * apclienable == 1 )
		//debug("debug 0");
		if(option_mode == 1)
			document.wireless_apcli_setup.apcli_enable.options.selectedIndex = 1;
		else
			document.wireless_apcli_setup.apcli_enable.options.selectedIndex = 2;
		document.getElementById("div_apcli_enable").style.visibility = "visible";
		document.getElementById("div_apcli_enable").style.display = "";
		document.wireless_apcli_setup.channel_sele_usa.selectedIndex = channel_index;
		if(1 * ext_channel == 1)
			document.wireless_apcli_setup.extchannel.selectedIndex = 1;
		else
			document.wireless_apcli_setup.extchannel.selectedIndex = 0;
		if(apcliencryptype == 'NONE')
			document.wireless_apcli_setup.authen_code_sele.selectedIndex = 0;
			encryp_dis();
		else if(apcliencryptype == 'WEP')
			//alert("wep 3");
			if (apcliauth == 'OPEN')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 1;
			else
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 2;
				
			var wep_key_length_tm;
			if(1 * apcliwepdefaultKey == 1)
			{
				//alert("key1");
				document.wireless_apcli_setup.wep_key_sele[0].checked = true;
				wep_key_length_tm = apcliwepkey1.length;
			}
			else if(1 * apcliwepdefaultKey == 2)
			{
				//alert("key2");
				document.wireless_apcli_setup.wep_key_sele[1].checked = true;
				wep_key_length_tm = apcliwepkey2.length;
			}
			else if(1 * apcliwepdefaultKey == 3)
			{
				//alert("key3");
				document.wireless_apcli_setup.wep_key_sele[2].checked = true;
				wep_key_length_tm = apcliwepkey3.length;
			}
			else if(1 * apcliwepdefaultKey == 4)
			{
				//alert("key4");
				document.wireless_apcli_setup.wep_key_sele[3].checked = true;
				wep_key_length_tm = apcliwepkey4.length;
			}
			else
			{
				//alert("key1 11");
				document.wireless_apcli_setup.wep_key_sele[0].checked = true;
				wep_key_length_tm = apcliwepkey1.length;
			}
				
			if(1 * apcli_key1_type == 1)//
			{
				document.wireless_apcli_setup.wep_key_meth[0].checked = true;
				
				if(wep_key_length_tm == 5)
				{//wep64
					
					document.getElementById("encry_wep64").checked = true;
					encryp_wep64();
					
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
					
				}
				else//wep128
				{
					document.getElementById("encry_wep128").checked = true;
					encryp_wep128();
				}
						
			}
			else //hex
			{
				document.wireless_apcli_setup.wep_key_meth[1].checked = true;
				if(wep_key_length_tm == 10)
				{//wep64
					document.getElementById("encry_wep64").checked = true;
					encryp_wep64();
				}
				else//wep128
				{
					document.getElementById("encry_wep128").checked = true;
					encryp_wep128();
				}
			}
			var tmpbuf1=apcliwepkey1;
			if(apcliwepkey1.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwepkey1.length;j++)
				tmpbuf1=tmpbuf1.replace('&quot;','"');
			}
			document.getElementById("wep5_key1").value = tmpbuf1;
			var tmpbuf2=apcliwepkey2;
			if(apcliwepkey2.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwepkey2.length;j++)
				tmpbuf2=tmpbuf2.replace('&quot;','"');
			}
			document.getElementById("wep5_key2").value = tmpbuf2;
			var tmpbuf3=apcliwepkey3;
			if(apcliwepkey3.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwepkey3.length;j++)
				tmpbuf3=tmpbuf3.replace('&quot;','"');
			}
			document.getElementById("wep5_key3").value = tmpbuf3;
			var tmpbuf4=apcliwepkey4;
			if(apcliwepkey4.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwepkey4.length;j++)
				tmpbuf4=tmpbuf4.replace('&quot;','"');
			}
			document.getElementById("wep5_key4").value = tmpbuf4;
		else if(apcliencryptype == "TKIP") 
			document.wireless_apcli_setup.encry_type[3].checked = true;
			if (apcliauth == 'WPAPSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 3;
			else if (apcliauth == 'WPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 4;
			var tmpbuf=apcliwpakey;
			if(apcliwpakey.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwpakey.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.wireless_apcli_setup.apcli_wpa_pass.value = tmpbuf;
			encryp_tkip_aes_mixed();
		else if(apcliencryptype == "AES")
			document.wireless_apcli_setup.encry_type[4].checked = true;
			if (apcliauth == 'WPAPSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 3;
			else if (apcliauth == 'WPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 4;
			else if (apcliauth == 'WPAPSKWPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 5;
				
			var tmpbuf=apcliwpakey;
			if(apcliwpakey.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwpakey.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.wireless_apcli_setup.apcli_wpa_pass.value = tmpbuf;
			encryp_tkip_aes_mixed();
		else if(apcliencryptype == "TKIPAES")
			document.wireless_apcli_setup.encry_type[5].checked = true;
			if (apcliauth == 'WPAPSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 3;
			else if (apcliauth == 'WPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 4;
			else if (apcliauth == 'WPAPSKWPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 5;
				
			var tmpbuf=apcliwpakey;
			if(apcliwpakey.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwpakey.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.wireless_apcli_setup.apcli_wpa_pass.value = tmpbuf;
			encryp_tkip_aes_mixed();
		else if(apcliencryptype == "AESTKIP")
			document.wireless_apcli_setup.encry_type[5].checked = true;
			if (apcliauth == 'WPAPSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 3;
			else if (apcliauth == 'WPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 4;
			else if (apcliauth == 'WPAPSKWPA2PSK')
				document.wireless_apcli_setup.authen_code_sele.selectedIndex = 5;
				
			var tmpbuf=apcliwpakey;
			if(apcliwpakey.indexOf('&quot;')!=-1){
				for(var j=0;j<apcliwpakey.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.wireless_apcli_setup.apcli_wpa_pass.value = tmpbuf;
			encryp_tkip_aes_mixed();
	else if(1 * wds_enable)
		document.wireless_apcli_setup.apcli_enable.options.selectedIndex = 3;
		document.getElementById("div_wds_add").style.visibility = "visible";
		document.getElementById("div_wds_add").style.display = "";
		document.getElementById("div_wds_list").style.visibility = "visible";
		document.getElementById("div_wds_list").style.display = "";
	else
		document.wireless_apcli_setup.apcli_enable.options.selectedIndex = 0;
	APClientEnableOnChange();
	auth_sele_init_change();
	if(wds_enable == 0)
		document.getElementById("bandwidth_ra1").checked =true;
	else
		document.getElementById("bandwidth_ra0").checked =true;
	var tmpbuf=apclissid;
	if(apclissid.indexOf('&quot;')!=-1){
		for(var j=0;j<apclissid.length;j++)
		tmpbuf=tmpbuf.replace('&quot;','"');
	document.wireless_apcli_setup.apcli_ssid.value = tmpbuf;
function init()
	var f=document.LAN;
	cfg2Form(f);
function IsHex(s) 
	var i;
	for( i = 0 ; i < s.length; i++ )
		if( !(
		((s.charAt(i) >= 'a') && (s.charAt(i) <= 'f')) || 
		((s.charAt(i) >= 'A' ) && (s.charAt(i) <= 'F')) || 
		((s.charAt(i) >= '0' ) && (s.charAt(i) <= '9')) ))
			return 1;
	return 0;
function Apply()
	var f=document.wireless_apcli_setup;
	if(f.apcli_enable.options.selectedIndex == 1)
		f.opmode.value  = 1;
		f.wantype.value = wan_type;
	else if(f.apcli_enable.options.selectedIndex == 2)
		f.opmode.value  = 2;
		f.wantype.value = 2;
	else
		f.wantype.value = wan_type;
	if(f.channel_sele_usa.selectedIndex == 0)
		f.AutoChannelSelect2.value = 2;
	else
		f.AutoChannelSelect2.value = 0;
	f.apclichannel.value = f.channel_sele_usa.selectedIndex;
	var ex_temp = 1 * (f.extchannel.selectedIndex);
	if(ex_temp == 0)
		f.extchannel_t.value =0;
	else if(ex_temp == 1)
		f.extchannel_t.value =1;
	if(document.wireless_apcli_setup.wep_key_sele[0] == true)
		f.ApCliDefaultKeyID2.value = 1;
	else if(document.wireless_apcli_setup.wep_key_sele[1] == true)
		f.ApCliDefaultKeyID2.value = 2;
	else if(document.wireless_apcli_setup.wep_key_sele[2] == true)
		f.ApCliDefaultKeyID2.value = 3;
	else if(document.wireless_apcli_setup.wep_key_sele[3] == true)
		f.ApCliDefaultKeyID2.value = 4;
	if(f.authen_code_sele.selectedIndex == 0)//dis
		f.apcliencryptype2.value = "NONE";
		f.apcliauth2.value = "OPEN";
		f.ApCliKey1Type.value = apcli_key1_type;
		f.ApCliKey2Type.value = apcli_key2_type;
		f.ApCliKey3Type.value = apcli_key3_type;
		f.ApCliKey4Type.value = apcli_key4_type;
		f.apcli_wep_key_1.value = apcliwepkey1;
		f.apcli_wep_key_2.value = apcliwepkey2;
		f.apcli_wep_key_3.value = apcliwepkey3;
		f.apcli_wep_key_4.value = apcliwepkey4;
		f.apcli_passphrase.value = apcliwpakey;
	else if(f.authen_code_sele.selectedIndex == 1)//open
		f.apcliencryptype2.value = "WEP";
		f.apcliauth2.value = "OPEN";
		if(f.wep_key_sele[0].checked == true)
			if(f.wep_key_meth[0].checked == true)//ascii
			{
				f.ApCliKey1Type.value = 1;
				if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
				{
					alert("
J!");
					f.wep5_key1.select();
					f.wep5_key1.focus();
				}	
			}
			else
			{
				f.ApCliKey1Type.value = 0;
				if(IsHex(f.wep5_key1.value))
				{
					alert("16
(0-9,a-f,A-F)
J!");
					return;
				}
			}
				
			f.ApCliKey2Type.value = apcli_key2_type;
			f.ApCliKey3Type.value = apcli_key3_type;
			f.ApCliKey4Type.value = apcli_key4_type;
		else if(f.wep_key_sele[1].checked == true)
