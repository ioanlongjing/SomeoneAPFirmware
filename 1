<SCRIPT language=JavaScript>
var oldpass_tm = ""; 
var adm_port = ""; 
var adm_rm_en = ""; 
var adm_rm_ip = ""; 
var oldpass_tm = "admin"; 
var adm_port = "8080"; 
var adm_rm_en = "0"; 
var adm_rm_ip = "0.0.0.0"; 
if(getCfg("RMIP")=="0.0.0.0") setCfg("RMIP"," ");
function init(){
	f=document.frmSetup;
	cfg2Form(f);
	document.frmSetup.SYSPS.value = "";
	document.frmSetup.oldpass.value = "";
function Cancel() {
	init();
	document.frmSetup.reset();
function Apply2() {
	//alert("1");
	f=document.frmSetup;
	f.RMPORT.value = adm_port;
	f.RMEN.value = adm_rm_en;
	var old_pass_str= f.oldpass.value;
	if(!(old_pass_str.indexOf(oldpass_tm)==0 && oldpass_tm.indexOf(old_pass_str)==0))
		alert("
T!!!") ;
		return;
	//alert("port:"+f.RMPORT.value);
	//if (!rangeCheck(f.RMPORT,1,65535,"
")) return;
	if (!chkStrLen(f.SYSPS,0,12,"
X")) return;
	//if (!chkPwdUpdate(f.SYSPS,f.SYSPS2,f.SYSPSC)) return;
	//alert("3");
	var str1 = f.SYSPS.value;
	var str2 = f.SYSPS2.value;
	if(!(str1.indexOf(str2)==0 && str2.indexOf(str1)==0))
		alert("
P!!!") ;
		return;
	else
		f.ADMINACC.value = f.SYSPS.value;
	form2Cfg(f);
	//if(f.RMIP.value=="*") setCfg("RMIP","0.0.0.0");
	subForm(f,'do_cmd.htm','SYS',cPage);
function Apply() {
	f=document.frmSetup;
	if (!rangeCheck(f.RMPORT,1,65535,"
")) return;
	//alert("3");
	f.SYSPS.value = f.SYSPS2.value = oldpass_tm;
	//alert("4");
	f.ADMINACC.value = f.SYSPS.value;
	if (!chkStrLen(f.SYSPS,0,12,"
X")) return;
	//if (!chkPwdUpdate(f.SYSPS,f.SYSPS2,f.SYSPSC)) return;
	form2Cfg(f);
	//if(f.RMIP.value=="*") setCfg("RMIP","0.0.0.0");
	subForm(f,'do_cmd.htm','DHCP',cPage);
function Help(){
	window.open("help_tools.htm#adm","help");
