						document.write("<td ><div class=rssi_low></div></td><td ><div  class=rssi_low2></div></td><td><div class=rssi_good></div></td><td><div style=\"width:"+rssi0_width+"px; height:20px;background-color:#005fbc;\"></div></td ><td class=p_font>"+rssi0+"%</td >");
					}
					else if(rssi0 >= 40)
					{
						//alert("up 40%..............\n");
						rssi0_width=rssi0 - 40;
						document.write("<td ><div class=rssi_low></div></td><td ><div  class=rssi_low2></div></td><td><div style=\"width:"+rssi0_width+"px; height:20px;background-color:#0083d2;\"></div></td><td class=p_font>"+rssi0+"%</td >");
