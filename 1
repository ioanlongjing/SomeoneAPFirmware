var wds_enable = "";
var wz_wds_list = "";
function CheckMAC1(mac_addr)
	var f=document.wds;
	var	i;
	var mac_tmp=mac_addr.split(":");
	var mac_addr2="";
	for(i=0;i<mac_tmp.length;i++)
	  mac_addr2+=mac_tmp[i];
    
	if(mac_addr2.length==0){
		alert("
uMAC
I");
		return false ;
	if ( mac_addr2.length != 12) {
		alert("
uMAC
I");
		return false;
  	}
  	if(mac_addr2 == "000000000000" || mac_addr2.toUpperCase() == "FFFFFFFFFFFF") {
		alert("
uMAC
G00:00:00:00:00:00 
 ff:ff:ff:ff:ff:ff
C");
		return false;
	if ( mac_addr2.length != 12) {
		alert("
uMAC
I");
		return false;
  	}
	if((mac_addr2.charAt(1)&0x01)||(mac_addr2.charAt(1).toUpperCase()=='B')||(mac_addr2.charAt(1).toUpperCase()=='D')||(mac_addr2.charAt(1).toUpperCase()=='F')){
		alert("
uMAC
C");
		return false;
	for(i = 0; i < 5; i++)
		if(isNaN(parseInt(mac_addr.charAt(3*i), 16)))
			alert('MAC
J.');
			return false;
		}	
		else if(isNaN(parseInt(mac_addr.charAt(3*i+1), 16)))
			alert('MAC
J.');
		    return false;
		}    
		else if (mac_addr.charAt(3*i+2) != ':')
			alert('MAC
J.');
			return false;     	
		}    
	if(isNaN(parseInt(mac_addr.charAt(15), 16)))
		alert('MAC
J.');
		return false;
	else if(isNaN(parseInt(mac_addr.charAt(16), 16)))
		alert('MAC
J.');
		return false;
	return true;
var phy_mode=Array("CCK","OFDM","HTMIX","GREENFIELD");
function trim(str){ 
return str.replace(/\s/g, ""); 
function add()
	var f=document.wds;
	var ap_bssid=f.ap_bssid1.value+":"+f.ap_bssid2.value+":"+f.ap_bssid3.value+":"+f.ap_bssid4.value+":"+f.ap_bssid5.value+":"+f.ap_bssid6.value;
	if(!CheckMAC1(ap_bssid))
		return;
	if(parseInt(trim(f.ap_bssid1.value),16)==255 && parseInt(trim(f.ap_bssid2.value))==255  && parseInt(trim(f.ap_bssid3.value))==255 && parseInt(trim(f.ap_bssid4.value))==255 && parseInt(trim(f.ap_bssid5.value))==255 && parseInt(trim(f.ap_bssid6.value))==255)
		alert("MAC
J!");
		return;
	if(parseInt(trim(f.ap_bssid1.value),16)==1 && parseInt(trim(f.ap_bssid2.value),16)==0 )
		alert("MAC
J!");
		return;
	var str1 = ap_bssid;
	var str2 = str;
	if(str2.indexOf(str1) >= 0)
		alert("MAC
b!") ;
		return;
	if(total_wds==0)
		f.WdsList2.value=ap_bssid+";;;";
	else if(total_wds==1)
		f.WdsList2.value=str.split(";")[0]+";"+ap_bssid+";;";
	else if(total_wds==2)
		f.WdsList2.value=str.split(";")[0]+";"+str.split(";")[1]+";"+ap_bssid+";";
	else if(total_wds==3)
		f.WdsList2.value=str.split(";")[0]+";"+str.split(";")[1]+";"+str.split(";")[2]+";"+ap_bssid;
	else
	     alert("
w 4 
!");
		 return;
	f.apcli_enable_tm.value = 0;
	f.WdsEnable3.value = 3;
	f.opmode3.value  = 1;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_APCLI_SETUP',cPage);
	return true;
function delect_all()
	var f=document.wds;
	var i=0,j=0;
	f.WdsList2.value = "";
	if(!total_wds)
		return;
	else if(total_wds == 1)
		if(f.wdsmac.checked == true)
			document.wds.WdsList2.value=";;;";
		else
			return;
	else if(total_wds == 2)
		j = 0;
		for(i=0;i<2;i++)
			if(f.wdsmac[i].checked == false)
			{
				f.WdsList2.value += wds_mac[i]+";";
				j++;
			}
		if(j == 0)
			document.wds.WdsList2.value=";;;";
		else if(j == 1)
			document.wds.WdsList2.value  += ";;";
		else 
			document.wds.WdsList2.value  += ";";
	else if(total_wds == 3)
		j = 0;
		for(i=0;i<3;i++)
			if(f.wdsmac[i].checked == false)
			{
				f.WdsList2.value += wds_mac[i]+";";
				j++;
			}
		if(j == 0)
			document.wds.WdsList2.value=";;;";
		else if(j == 1)
			document.wds.WdsList2.value  += ";;";
		else if(j == 2)
			document.wds.WdsList2.value  += ";";
	else if(total_wds == 4)
		j = 0;
		for(i=0;i<4;i++)
			if(f.wdsmac[i].checked == false)
			{
				if(j == 3)
					f.WdsList2.value += wds_mac[i];
				else
					f.WdsList2.value += wds_mac[i]+";";
				j++;
			}
		if(j == 0)
			document.wds.WdsList2.value=";;;";
		else if(j == 1)
			document.wds.WdsList2.value  += ";;";
		else if(j == 2)
			document.wds.WdsList2.value  += ";";
	f.WdsPhyMode2.value = "CCK;CCK;CCK;CCK";
	f.WdsEnable3.value = 3;
	form2Cfg(f);
	//return;
	subForm(f,'do_cmd.htm','DHCP',cPage);
	return true;
function apply_wds()
	var f=document.wds_dis;
	if(document.getElementById("bandwidth_ra0").checked == true)
		setCfg("WdsEnable",3);
	else
		setCfg("WdsEnable",0);
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP',cPage);
function openAPlistwin()
	//window.open('aplist.htm');
