	statusStr = statStr[conStat];
function preSubmit(idx) {
	var f=document.frmSetup;
	f.action.value=idx
	f.submit() ;
function init()
if (conStat!=1)
	setTimeout('location="lan_wan_status.htm"',5000);
function formatStr(num,len)
	var nbits = new String(num);
	var rst = "";
	a = len - (nbits.length)
	if ( a > 0)
	 for (i=0;i<a;i++)
		rst += "0";
	 rst += num;
	else
	 rst = num;
	return rst
function parseTime(t)
	var str=new String("");
	var tmp=parseInt(t);
	var sec=0,min=0,hr=0,day=0;
	sec=t % 60;  //sec
	min=parseInt(t/60) % 60; //min
	hr=parseInt(t/(60*60)) % 24; //hr
	day=parseInt(t/(60*60*24)); //day
	str=(day >0? day+" Day,  ":"0 Day,  ")+(hr >0? formatStr(hr,2)+":":"00:")+(min >0? formatStr(min,2)+":":"00:")+(sec >0? formatStr(sec,2)+"":"00");
	return str;
function genHTML()
	if (dhcpServer == 1)
	  enableStr = "Enable";
	else
	  enableStr = "Disable";
	var modeStr = typeStr[conType];
	switch(Number(conType))
		case 1:
			statusStr = "";
			break;
		case 2:
			statusStr+="<input type=button onClick=preSubmit(1); value=\"DHCP Release\" style=\"visibility: visible; display: none;\"> <input type=button onClick=preSubmit(2); value=\"DHCP Renew\" style=\"visibility: visible; display: none;\">";
			break;
		case 3:
			if (pppoeMode == "1") modeStr = "Fixed "+modeStr;
			statusStr+="<input type=button onClick=preSubmit(3); value=Connect style=\"visibility: visible; display: none;\"> <input type=button onClick=preSubmit(4); value=Disconnectstyle=\"visibility: visible; display: none;\" >";
			break;
		case 4:
			statusStr+="<input type=button onClick=preSubmit(5); value=Connect style=\"visibility: visible; display: none;\"> <input type=button onClick=preSubmit(6); value=Disconnect style=\"visibility: visible; display: none;\">";
			break;
		case 5:
			statusStr+="<input type=button onClick=preSubmit(7); value=Connect style=\"visibility: visible; display: none;\"> <input type=button onClick=preSubmit(8); value=Disconnect style=\"visibility: visible; display: none;\">";
			break;
		default:
			modeStr = "";
			statusStr = "";
			break;
	var str = "";
	// LAN
	str+="<tr><td colspan=2 align=left class=web_title2>LAN</td></tr>";
	str+="<tr><td width=30% class=itemdesc_td height=25 >MAC Address</td>";
	str+="<td width=70% class=\"item_td\">"+lan_mac+"</td></tr>";
	str+="<tr><td  class=itemdesc_td height=25 >IP Address</td>";
	str+="<td width=70% class=\"item_td\">"+ipAddr+"</td></tr>";
	str+="<tr><td  class=itemdesc_td height=25 >Subnet Mask</td>";
	str+="<td width=70% class=\"item_td\">"+gateSubMsk+"</td></tr>";
	str+="<tr><td class=itemdesc_td height=25 >DHCP Server</td>";
	str+="<td width=70% class=\"item_td\">"+enableStr+"</td></tr>";
	// WAN
	str+="<tr><td colspan=2>&nbsp;</td></tr>";
	str+="<tr><td colspan=2 class=web_title2>WAN</td></tr>";
	str+="<tr><td class=itemdesc_td height=25 >MAC Address</td>";
	str+="	<td class=\"item_td\">"+wan_mac+"</td></tr>";
	str+="<tr><td class=itemdesc_td height=25 >Connection</td>";
	str+="	<td class=\"item_td\">"+modeStr+" "+statusStr+"</td></tr>";
	str+="<tr><td class=itemdesc_td height=25 >IP Address</td>";
	str+="	<td class=\"item_td\">"+wanIP+"</td></tr>";
	str+="<tr><td class=itemdesc_td height=25 >Subnet Mask</td>";
	str+="	<td class=\"item_td\">"+subMask+"</td></tr>";
