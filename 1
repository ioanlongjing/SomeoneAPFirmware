	str1 += "<input type=button style=\"{width:70px;}\" value=
 name=Edit2 class=p_font onclick=Editssid2(2)></td>";
	str1 += "</tr>";
	document.write(str);
	document.write(str1);
else if(bra_ssid_num == 3)
	var str = "";
	var str1 = "";
	var str2 = "";
	str += "<tr><td class=item_td ><img src='images/radio.gif'></td>";
	if(secu_mode.split(";")[0] == 0 )
