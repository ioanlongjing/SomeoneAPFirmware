function isIpSubnet(s1,mn,s2){
  	var ip1=s1.split(".");
   	var ip2=s2.split(".");
   	var ip3=mn.split(".");
   	//if(ip1.length!=4||ip2.length!=4||ip3.length!=4) return 0;
   	for(var k=0;k<=3;k++){
		if((ip1[k]&ip3[k])!=(ip2[k]&ip3[k])) return 0;
   	return 1;
function Apply() {
	var f=document.SIP;
	var ip,gw;
	f.WANMSK.value = trim(f.WANMSK.value);
	f.WANIP.value = trim(f.WANIP.value);
	f.WANGW.value = trim(f.WANGW.value);
	ip = f.WANIP.value;
	gw = f.WANGW.value;
	//.split(";")[2]
	if(!(ip.split(".")[0] == gw.split(".")[0] && ip.split(".")[1] == gw.split(".")[1] && ip.split(".")[2] == gw.split(".")[2]))
		alert("IP
J!");
		return;
	if (!isIpSubnet(f.WANIP.value, f.WANMSK.value, f.WANGW.value))
		alert("IP
