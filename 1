<script>
ssid_s1 = "totolink";
ssid_s2 = "mssid1";
ssid_s3 = "mssid2";
secu_mode = "1;0;3;";
auth_mode = "OPEN;OPEN;WPA2PSK;OPEN";
encry_type = "NONE;WEP;TKIP;NONE";
bra_ssid_num = 3;
if(bra_ssid_num == 1)
	var str = "";
	str += "<tr><td class=item_td ><img src='images/radio.gif'></td>";
	if(secu_mode.split(";")[0] == 0 )
		str += "<td class=item_td colspan=2 align=left>"+ssid_s1.replace(eval("/ /gi"),'&nbsp;')+"<br></td>";
	else
		str += "<td class=item_td colspan=2>" + ssid_s1.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+ auth_mode.split(";")[0] + "-" + encry_type.split(";")[0] + ")</font></td>";
	str += "</tr>";
	document.write(str);
else if(bra_ssid_num == 2)
	var str = "";
	var str1 = "";
	str += "<tr><td class=item_td width=5%><img src='images/radio.gif'></td>";
	if(secu_mode.split(";")[0] == 0 )
		str += "<td class=item_td colspan=2 align=left>"+ssid_s1.replace(eval("/ /gi"),'&nbsp;')+"<br></td>";
	else 
		str += "<td class=item_td colspan=2>" + ssid_s1.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+ auth_mode.split(";")[0] + "-" + encry_type.split(";")[0] + ")</font></td>";
	str += "</tr>";
	str1 += "<tr><td class=item_td><img src='images/radio.gif'></td>";
	//str1 += ssid_s2+"<br>";
	if(secu_mode.split(";")[1] == 0 )
		str1 += "<td class=item_td >"+ ssid_s2.replace(eval("/ /gi"),'&nbsp;') +"</td>";
	else 
		str1 += "<td class=item_td >"  + ssid_s2.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+auth_mode.split(";")[1]+"-"+encry_type.split(";")[1]+")</font></td>";
