function IsHex(s) 
	var i;
	for( i = 0 ; i < s.length; i++ )
		if( !(
		((s.charAt(i) >= 'a') && (s.charAt(i) <= 'f')) || 
		((s.charAt(i) >= 'A' ) && (s.charAt(i) <= 'F')) || 
		((s.charAt(i) >= '0' ) && (s.charAt(i) <= '9')) ))
			return 1;
	return 0;
function wep_key_meth_ascii_change()
	//alert("ascii");
	if(document.wireless_basic.encry_type[1].checked == true)//wep64
		document.getElementById("wep5_key").maxLength = 5;	
		document.getElementById("wep5_key").size = 5;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
	else if(document.wireless_basic.encry_type[2].checked == true)//wep128
		document.getElementById("wep5_key").maxLength = 13;	
		document.getElementById("wep5_key").size = 16;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
function wep_key_meth_hex_change()
	//alert("hex");
	if(document.wireless_basic.encry_type[1].checked == true)//wep64
		document.getElementById("wep5_key").maxLength = 10;	
		document.getElementById("wep5_key").size = 10;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
	else if(document.wireless_basic.encry_type[2].checked == true)//wep128
		document.getElementById("wep5_key").maxLength = 26;	
		document.getElementById("wep5_key").size = 32;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
function encryp_wep64()
	//alert("wep64");
	document.getElementById("div_wep").style.visibility = "visible";
	document.getElementById("div_wep").style.display = "";
	var keytype = 0;//1 ascii :0 hex
	var f=document.wireless_basic;
	if(f.wep_key_meth[0].checked == true)
		keytype = 1;
	else
		keytype = 0;
	if(keytype == 0 )
		document.getElementById("wep5_key").maxLength = 10;	
		document.getElementById("wep5_key").size = 10;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
	else
		document.getElementById("wep5_key").maxLength = 5;	
		document.getElementById("wep5_key").size = 5;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
	document.getElementById("div_wpa").style.visibility = "hidden";
	document.getElementById("div_wpa").style.display = "none";
	document.getElementById("wep5_key").value = "";
function encryp_wep128()
	//alert("wep128");
	document.getElementById("div_wep").style.visibility = "visible";
	document.getElementById("div_wep").style.display = "";
	var keytype = 0;//1 ascii :0 hex
	var f=document.wireless_basic;
	if(f.wep_key_meth[0].checked == true)
		keytype = 1;
	else
		keytype = 0;
	if(keytype == 0)//hex
		document.getElementById("wep5_key").maxLength = 26;	
		document.getElementById("wep5_key").size = 32;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
	else if(keytype == 1)
		document.getElementById("wep5_key").maxLength = 13;	
		document.getElementById("wep5_key").size = 13;
		document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
	document.getElementById("div_wpa").style.visibility = "hidden";
	document.getElementById("div_wpa").style.display = "none";
	document.getElementById("wep5_key").value = "";
function encryp_tkip_aes_mixed()
	document.getElementById("div_wpa").style.visibility = "visible";
	document.getElementById("div_wpa").style.display = "";
	document.getElementById("div_wep").style.visibility = "hidden";
	document.getElementById("div_wep").style.display = "none";
function auth_sele_change()
	if(document.getElementById("encry__sele").options.selectedIndex == 0)
		document.getElementById("wep5_key").value = "";
		document.wireless_basic.encry_type[0].checked = true;
		document.getElementById("encry_dis").disabled = false;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
	else if(document.getElementById("encry__sele").options.selectedIndex == 1)
		if(document.wireless_basic.encry_type[1].checked == false && document.wireless_basic.encry_type[2].checked == false)
			document.wireless_basic.encry_type[1].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		if(document.wireless_basic.encry_type[1].checked == true)
			encryp_wep64();
		else
			encryp_wep128();
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
		document.getElementById("wep5_key").value = "";
	else if(document.getElementById("encry__sele").options.selectedIndex == 2)
		if(document.wireless_basic.encry_type[1].checked == false && document.wireless_basic.encry_type[2].checked == false)
			document.wireless_basic.encry_type[1].checked = true;
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = false;
		document.getElementById("encry_wep128").disabled = false;
		document.getElementById("encry_tkip").disabled = true;
		document.getElementById("encry_aes").disabled = true;
		document.getElementById("encry_tkip_aes").disabled = true;
		if(document.wireless_basic.encry_type[1].checked == true)
			encryp_wep64();
		else
			encryp_wep128();
		document.getElementById("div_wep").style.visibility = "visible";
		document.getElementById("div_wep").style.display = "";
		document.getElementById("div_wpa").style.visibility = "hidden";
		document.getElementById("div_wpa").style.display = "none";
		document.getElementById("wep5_key").value = "";
	else 
		document.getElementById("encry_dis").disabled = true;
		document.getElementById("encry_wep64").disabled = true;
		document.getElementById("encry_wep128").disabled = true;
		document.getElementById("encry_tkip").disabled = false;
		document.getElementById("encry_aes").disabled = false;
		document.getElementById("encry_tkip_aes").disabled = true;
		document.getElementById("div_wpa").style.visibility = "visible";
		document.getElementById("div_wpa").style.display = "";
		document.getElementById("div_wep").style.visibility = "hidden";
		document.getElementById("div_wep").style.display = "none";
		if(document.getElementById("encry__sele").options.selectedIndex == 3)
			document.wireless_basic.encry_type[3].checked = true;
		else if(document.getElementById("encry__sele").options.selectedIndex == 4) 
			document.wireless_basic.encry_type[4].checked = true;
		else
			document.wireless_basic.encry_type[5].checked = true;
			document.getElementById("encry_tkip_aes").disabled = false;
function inition()
	document.getElementById("encry__sele").options.selectedIndex = 0;
	auth_sele_change();
function Del_ssid()
	var f=document.wireless_basic;
	//var ssid_num = 1 * bra_ssid_num;
	var edit_flag = 0;
	if(ssid_num_id ==1)
		return;
	if(ssid_num_id ==2)
		if(f.ssid2check.checked == true)
			f.Bssid_num.value = 1;
			f.mssid_1.value = ssid_s2;
			f.mssid_2.value = "";
		else 
			return;
	else if(ssid_num_id ==3)
		if(f.ssid2check.checked == true && f.ssid3check.checked == true)
			f.Bssid_num.value = 1;	
			f.mssid_1.value = "";
			f.mssid_2.value = "";
		else if(f.ssid2check.checked == true)
		{	
			//alert("ssid 2!ssid_s3::"+ssid_s3);
			f.mssid_1.value = ssid_s3;
			f.mssid_2.value = "";
			f.Bssid_num.value = 2;
			edit_flag = 1;
		else if(f.ssid3check.checked == true)
			f.Bssid_num.value = 2;
			f.mssid_1.value = ssid_s2;
			f.mssid_2.value = "";
		else
			return;
	else
		f.Bssid_num.value = 1;
	if(edit_flag == 0)
		f.AuthMode.value = auth_mode;
		f.EncrypType.value = encry_type;
		f.HideSSID2.value = broadcastssidEnable;
		f.SecurityMode2.value = secu_mode;
		setCfg("WepKeyLength2",wep_key_len);
		setCfg("WepDefaultKey2",wep_defau_key);
	else
		f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[2]+";"+auth_mode.split(";")[2];
		f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[2]+";"+encry_type.split(";")[2];
		f.HideSSID2.value = broadcastssidEnable.split(";")[0]+";"+broadcastssidEnable.split(";")[2]+";"+broadcastssidEnable.split(";")[2];
		f.SecurityMode2.value =  secu_mode.split(";")[0]+";"+secu_mode.split(";")[2]+";"+secu_mode.split(";")[2];
		setCfg("Key1Str2Temp",key1_str3);
		setCfg("Key2Str2Temp",key2_str3);
		setCfg("Key3Str2Temp",key3_str3);
		setCfg("Key4Str2Temp",key4_str3);
		setCfg("WPAPSK2Temp",wpa_key3);
		setCfg("WepKeyLength2",wep_key_len.split(";")[0]+";"+wep_key_len.split(";")[2]+";"+wep_key_len.split(";")[2]);
		setCfg("WepDefaultKey2",wep_defau_key.split(";")[0]+";"+wep_defau_key.split(";")[2]+";"+wep_defau_key.split(";")[2]);
		setCfg("Key1TypeTemp",key1type.split(";")[0]+";"+key1type.split(";")[2]+";"+key1type.split(";")[2]);
		setCfg("Key2TypeTemp",key2type.split(";")[0]+";"+key2type.split(";")[2]+";"+key2type.split(";")[2]);
		setCfg("Key3TypeTemp",key3type.split(";")[0]+";"+key3type.split(";")[2]+";"+key3type.split(";")[2]);
		setCfg("Key4TypeTemp",key4type.split(";")[0]+";"+key4type.split(";")[2]+";"+key4type.split(";")[2]);
	alert("Bssid_num:"+f.Bssid_num.value);
	alert("mssid_1:"+f.mssid_1.value);
	alert("mssid_2:"+f.mssid_2.value);
	alert("AuthMode:"+f.AuthMode.value);
	alert("EncrypType:"+f.EncrypType.value);
	alert("HideSSID2:"+f.HideSSID2.value);
	alert("SecurityMode2:"+f.SecurityMode2.value);
	alert("Key1TypeTemp:"+f.Key1TypeTemp.value);
	alert("Key2TypeTemp:"+f.Key2TypeTemp.value);
	alert("Key3TypeTemp:"+f.Key3TypeTemp.value);
	alert("Key4TypeTemp:"+f.Key4TypeTemp.value);
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP',cPage);
	return true;
function apply()
	var ssid_nu = 1 * bra_ssid_num;
	var f=document.wireless_basic;
	var Key1TypeTemp_st;
	var Key2TypeTemp_st;
	var Key3TypeTemp_st;
	var Key4TypeTemp_st;
	var Key1Str2Temp_st;
	var Key1Str3Temp_st;
	var Key2Str2Temp_st;
	var Key2Str3Temp_st;
	var Key3Str2Temp_st;
	var Key3Str3Temp_st;
	var Key4Str2Temp_st;
	var Key4Str3Temp_st;
	var WepKeyLength2_st;
	var WepDefaultKey2_st;
	var WPAPSK2Temp_st;
	var WPAPSK3Temp_st;
	if(f.ssid.value == '')
		alert("SSID
J!");
		return;
	else if(/.*[^\x00-\xff]+.*$/.test(f.ssid.value))
		alert("SSID
J!");
		f.ssid.select();
		return;
	/*else if ((f.ssid.value.indexOf("'")!=-1)||(f.ssid.value.indexOf('"')!=-1)||
		   (f.ssid.value.indexOf("#")!=-1)||(f.ssid.value.indexOf("!")!=-1)||
		   (f.ssid.value.indexOf("^")!=-1)||(f.ssid.value.indexOf("|")!=-1))
			alert("SSID
[' \" # ! ^ |],
J!");
			f.ssid.select();
			return;
	}*/
	if(!add_edit)
		//alert("1111111111111111111111");
		ssid_num_id = ssid_num_id + 1;
		//alert("ssid_num_id:::"+ssid_num_id);
		if(ssid_num_id > 3)
			alert("SSID
!");
			return;
		//Bssid_num
		f.Bssid_num.value = ssid_num_id;
	else
		ssid_num_id = add_edit;
		//Bssid_num
		if(ssid_nu == 2)
			f.Bssid_num.value = ssid_num_id;
		else
			f.Bssid_num.value = ssid_nu;
	//at last add_edit = 0;
	//add_edit = 0;
	//mssid_1
	//mssid_2
	if(ssid_num_id == 2)
		f.mssid_1.value = f.ssid.value;
		f.mssid_2.value = ssid_s3;
		//alert("ssid_num_id:::"+ssid_num_id+":::ssid::"+f.mssid_1.value);
	else if(ssid_num_id == 3)
		f.mssid_1.value = ssid_s2;
		f.mssid_2.value = f.ssid.value;
	//HideSSID2
	if(ssid_num_id == 2)
		if(f.broadcastssid[0].checked == true)
			f.HideSSID2.value = broadcastssidEnable.split(";")[0] + ";0;" + broadcastssidEnable.split(";")[2] ;
		else
			f.HideSSID2.value = broadcastssidEnable.split(";")[0]  + ";1;"+ broadcastssidEnable.split(";")[2] ;
	else if(ssid_num_id == 3)
		if(f.broadcastssid[0].checked == true)
			f.HideSSID2.value = broadcastssidEnable.split(";")[0] + ";" + broadcastssidEnable.split(";")[1] + ";0";
		else
			f.HideSSID2.value = broadcastssidEnable.split(";")[0] + ";" + broadcastssidEnable.split(";")[1] + ";1";
	//SecurityMode2
	//AuthMode
	//EncrypType
	//WepKeyLength2 0--->64 1--->128
	//WepDefaultKey2
	//Key1TypeTemp
	//Key2TypeTemp
	//Key3TypeTemp
	//Key4TypeTemp
	//Key1Str2Temp
	//Key1Str3Temp
	//Key2Str2Temp
	//Key2Str3Temp
	//Key3Str2Temp
	//Key3Str3Temp
	//Key4Str2Temp
	//Key4Str3Temp
	//WPAPSK2Temp
	//WPAPSK3Temp
	if(f.encry_type[0].checked == true)//dis
		if(ssid_num_id == 2)
			f.SecurityMode2.value = secu_mode.split(";")[0] + ";0;" + secu_mode.split(";")[2] ;
			f.AuthMode.value =auth_mode.split(";")[0]+";OPEN;"+auth_mode.split(";")[2];
			//f.EncrypType.value = "NONE;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2]+";"+encry_type.split(";")[3];
			f.EncrypType.value = encry_type.split(";")[0]+";NONE;"+encry_type.split(";")[2];
				
		else if(ssid_num_id == 3)
			f.SecurityMode2.value = secu_mode.split(";")[0] +";"+ secu_mode.split(";")[1] + ";0" ;
			f.AuthMode.value =auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";OPEN";
			f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";NONE";
	else if(f.authen_code_sele.options.selectedIndex == 1 || f.authen_code_sele.options.selectedIndex == 2 )//open shared
		if(ssid_num_id == 2)
			if(f.authen_code_sele.options.selectedIndex == 1 )
			{
			f.SecurityMode2.value = secu_mode.split(";")[0] + ";1;" + secu_mode.split(";")[2] ;
			f.AuthMode.value = auth_mode.split(";")[0]+";OPEN;"+auth_mode.split(";")[2];
			//f.EncrypType.value = "WEP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			f.EncrypType.value = encry_type.split(";")[0]+";WEP;"+encry_type.split(";")[2];
			}
			else
			{
				//alert("222222");
			f.SecurityMode2.value = secu_mode.split(";")[0] + ";1;" + secu_mode.split(";")[2] ;
			//f.SecurityMode2.value = 1;
			f.AuthMode.value =auth_mode.split(";")[0]+";SHARED;"+auth_mode.split(";")[2];
			//f.EncrypType.value = "WEP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			f.EncrypType.value = encry_type.split(";")[0]+";WEP;"+encry_type.split(";")[2];
			}
			if(f.encry_type[1].checked == true)//wep64
			{
				WepKeyLength2_st = wep_key_len.split(";")[0]+";0;"+wep_key_len.split(";")[2];
				
				if(f.wep_key_meth[0].checked == true)//ascii 5
				{
					if(f.wep5_key1.value.length != 5)
					{
						alert("
5!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
					{
						alert("
J!");
						f.wep5_key1.select();
						ssid_num_id = ssid_num_id - 1;
						return;
					}					
				}
				else//hex 10
				{
					if(f.wep5_key1.value.length != 10)
					{
						alert("
10!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					if(IsHex(f.wep5_key1.value))
					{
						alert("16
(0-9,a-f,A-F)
J!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
				}
			}	
			else if(f.encry_type[2].checked == true)//wep128
			{
				WepKeyLength2_st = wep_key_len.split(";")[0]+";1;"+wep_key_len.split(";")[2];
				
				if(f.wep_key_meth[0].checked == true)//ascii 13
				{
					if(f.wep5_key1.value.length != 13)
					{
						alert("
13!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
					{
						alert("
J!");
						f.wep5_key1.select();
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
				}
				else//hex 26
				{
					if(f.wep5_key1.value.length != 26)
					{
					alert("
26!");
					ssid_num_id = ssid_num_id - 1;
					return;
					}
					
					if(IsHex(f.wep5_key1.value))
					{
						alert("16
(0-9,a-f,A-F)
J!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
				}
			}
			if(f.wep_key_meth[0].checked == true)//ASCII--->1
			{
					Key1TypeTemp_st = key1type.split(";")[0]+";1"+";"+key1type.split(";")[2];
					Key2TypeTemp_st = key2type.split(";")[0]+";1"+";"+key2type.split(";")[2];
					Key3TypeTemp_st = key3type.split(";")[0]+";1"+";"+key3type.split(";")[2];
					Key4TypeTemp_st = key4type.split(";")[0]+";1"+";"+key4type.split(";")[2];
			}
			else //hex--->0
			{
					Key1TypeTemp_st = key1type.split(";")[0]+";0"+";"+key1type.split(";")[2];
					Key2TypeTemp_st = key2type.split(";")[0]+";0"+";"+key2type.split(";")[2];
					Key3TypeTemp_st = key3type.split(";")[0]+";0"+";"+key3type.split(";")[2];
					Key4TypeTemp_st = key4type.split(";")[0]+";0"+";"+key4type.split(";")[2];
			}
			if(document.getElementById("wep_key_sele1").checked == true)//key1
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";1"+";"+wep_defau_key.split(";")[2];
				Key1Str2Temp_st = f.wep5_key1.value ;
				setCfg("Key1Str2Temp",Key1Str2Temp_st);
				setCfg("Key1TypeTemp",Key1TypeTemp_st);
			}
			else if(document.getElementById("wep_key_sele2").checked == true)//key2
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";2"+";"+wep_defau_key.split(";")[2];
				Key2Str2Temp_st = f.wep5_key1.value ;
				setCfg("Key2Str2Temp",Key2Str2Temp_st);
				setCfg("Key2TypeTemp",Key2TypeTemp_st);
			}
			else if(document.getElementById("wep_key_sele3").checked == true)//key3
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";3"+";"+wep_defau_key.split(";")[2];
				Key3Str2Temp_st = f.wep5_key1.value;
				setCfg("Key3Str2Temp",Key3Str2Temp_st);
				setCfg("Key3TypeTemp",Key3TypeTemp_st);
			}
			else //key4 
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";4"+";"+wep_defau_key.split(";")[2];
				Key4Str2Temp_st = f.wep5_key1.value;
				setCfg("Key4Str2Temp",Key4Str2Temp_st);
				setCfg("Key4TypeTemp",Key4TypeTemp_st);
			}
				
			setCfg("WepKeyLength2",WepKeyLength2_st);
			setCfg("WepDefaultKey2",WepDefaultKey2_st);
		else if(ssid_num_id == 3)
			if(f.authen_code_sele.options.selectedIndex == 1)
			{
			f.SecurityMode2.value = secu_mode.split(";")[0] +";"+ secu_mode.split(";")[1] + ";1" ;
			f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";OPEN";
			f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";WEP";
			}
			else
			{
				f.SecurityMode2.value = secu_mode.split(";")[0] +";"+ secu_mode.split(";")[1] + ";1" ;
				f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";SHARED";
				f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";WEP";
			}
			if(f.encry_type[1].checked == true)//wep64
			{
				WepKeyLength2_st = wep_key_len.split(";")[0]+";"+wep_key_len.split(";")[1]+";0";
				
				if(f.wep_key_meth[0].checked == true)//wep5
				{
					if(f.wep5_key1.value.length != 5)
					{
						alert("
5!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
					{
						alert("
J!");
						f.wep5_key1.select();
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
				}
				else//wep10
				{
					if(f.wep5_key1.value.length != 10)
					{
						alert("
10!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
					if(IsHex(f.wep5_key1.value))
					{
						alert("16
(0-9,a-f,A-F)
J!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					
				}
			}	
			else if(f.encry_type[2].checked == true)//wep128
			{
				WepKeyLength2_st = wep_key_len.split(";")[0]+";"+wep_key_len.split(";")[1]+";1";
				if(f.wep_key_meth[0].checked == true)//wep13
				{
					if(f.wep5_key1.value.length != 13)
					{
						alert("
13!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
					else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
					{
						alert("
J!");
						f.wep5_key1.select();
						ssid_num_id = ssid_num_id - 1;
						return;
					}				
				}
				else//wep26
				{
					if(f.wep5_key1.value.length != 26)
					{
					alert("
26!");
					ssid_num_id = ssid_num_id - 1;
					return;
					}
					
					if(IsHex(f.wep5_key1.value))
					{
						alert("16
(0-9,a-f,A-F)
J!");
						ssid_num_id = ssid_num_id - 1;
						return;
					}
				
				}
			}
			if(f.wep_key_meth[0].checked == true)//ASCII--->1
			{
					Key1TypeTemp_st = key1type.split(";")[0]+";"+key1type.split(";")[1]+";1";
					Key2TypeTemp_st = key2type.split(";")[0]+";"+key2type.split(";")[1]+";1";
					Key3TypeTemp_st = key3type.split(";")[0]+";"+key3type.split(";")[1]+";1";
					Key4TypeTemp_st = key4type.split(";")[0]+";"+key4type.split(";")[1]+";1";
			}
			else //hex--->0
			{
					Key1TypeTemp_st = key1type.split(";")[0]+";"+key1type.split(";")[1]+";0";
					Key2TypeTemp_st = key2type.split(";")[0]+";"+key2type.split(";")[1]+";0";
					Key3TypeTemp_st = key3type.split(";")[0]+";"+key3type.split(";")[1]+";0";
					Key4TypeTemp_st = key4type.split(";")[0]+";"+key4type.split(";")[1]+";0";
			}
			if(document.getElementById("wep_key_sele1").checked == true)//key1
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";"+wep_defau_key.split(";")[1]+";1";
				Key1Str3Temp_st = f.wep5_key1.value ;
				setCfg("Key1Str3Temp",Key1Str3Temp_st);
				setCfg("Key1TypeTemp",Key1TypeTemp_st);
			}
			else if(document.getElementById("wep_key_sele2").checked == true)//key2
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";"+wep_defau_key.split(";")[1]+";2";
				Key2Str3Temp_st = f.wep5_key1.value ;
				setCfg("Key2Str3Temp",Key2Str3Temp_st);
				setCfg("Key2TypeTemp",Key2TypeTemp_st);
			}
			else if(document.getElementById("wep_key_sele3").checked == true)//key3
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";"+wep_defau_key.split(";")[1]+";3";
				Key3Str3Temp_st = f.wep5_key1.value;
				setCfg("Key3Str3Temp",Key3Str3Temp_st);
				setCfg("Key3TypeTemp",Key3TypeTemp_st);
			}
			else //key4 
			{
				WepDefaultKey2_st = wep_defau_key.split(";")[0]+";"+wep_defau_key.split(";")[1]+";4";
				Key4Str3Temp_st = f.wep5_key1.value;
				setCfg("Key4Str3Temp",Key4Str3Temp_st);
				setCfg("Key4TypeTemp",Key4TypeTemp_st);
			}
				
			setCfg("WepKeyLength2",WepKeyLength2_st);
			setCfg("WepDefaultKey2",WepDefaultKey2_st);
		}//end ssid_num_id == 3
	}//end open
	else if(f.authen_code_sele.options.selectedIndex == 3 || f.authen_code_sele.options.selectedIndex == 4 || f.authen_code_sele.options.selectedIndex == 5)//wpapsk
		if(ssid_num_id == 2)
			if(f.authen_code_sele.options.selectedIndex == 3)
			{
				f.SecurityMode2.value = secu_mode.split(";")[0] + ";3;" + secu_mode.split(";")[2] ;
				f.AuthMode.value =auth_mode.split(";")[0]+";WPAPSK;"+auth_mode.split(";")[2];
			}
			else if(f.authen_code_sele.options.selectedIndex == 4)
			{
				f.SecurityMode2.value = secu_mode.split(";")[0] + ";3;" + secu_mode.split(";")[2] ;
				f.AuthMode.value =auth_mode.split(";")[0]+";WPA2PSK;"+auth_mode.split(";")[2];
			}
			else
			{
				f.SecurityMode2.value = secu_mode.split(";")[0] + ";3;" + secu_mode.split(";")[2] ;
				f.AuthMode.value =auth_mode.split(";")[0]+";WPAPSKWPA2PSK;"+auth_mode.split(";")[2];
			}
			if(f.encry_type[3].checked == true)
				f.EncrypType.value = encry_type.split(";")[0]+";TKIP;"+encry_type.split(";")[2];
			else if(f.encry_type[4].checked == true)
				f.EncrypType.value = encry_type.split(";")[0]+";AES;"+encry_type.split(";")[2];
			else 
				f.EncrypType.value = encry_type.split(";")[0]+";TKIPAES;"+encry_type.split(";")[2];
				
			if(f.wpa_key.value.length < 8)
			{
				ssid_num_id = ssid_num_id - 1;
				alert("
8!");
				f.wpa_key.focus();
				f.wpa_key.select();
				return;
			}
			else if(/.*[^\x00-\xff]+.*$/.test(f.wpa_key.value))
			{
				alert("
J!");
				f.wpa_key.select();
				ssid_num_id = ssid_num_id - 1;
				return;
			}
			else if (f.wpa_key.value.length == 64 && !isHex(f.wpa_key.value))
			{
				ssid_num_id = ssid_num_id - 1;
				alert("16
(0-9,a-f,A-F)
J!");
				f.wpa_key.focus();
				f.wpa_key.select();
				return;
			}
			setCfg("WPAPSK2Temp",f.wpa_key.value);
		else if(ssid_num_id == 3)
			f.SecurityMode2.value = secu_mode.split(";")[0] +";"+ secu_mode.split(";")[1] + ";3" ;
			if(f.authen_code_sele.options.selectedIndex == 3)
				f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";WPAPSK";
			else if(f.authen_code_sele.options.selectedIndex == 4)
				f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";WPA2PSK";
			else
				f.AuthMode.value = auth_mode.split(";")[0]+";"+auth_mode.split(";")[1]+";WPAPSKWPA2PSK";
			if(f.encry_type[3].checked == true)
				f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";TKIP";
			else if(f.encry_type[4].checked == true)
				f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";AES";
			else 
				f.EncrypType.value = encry_type.split(";")[0]+";"+encry_type.split(";")[1]+";TKIPAES";
			if(f.wpa_key.value.length < 8)
			{
				alert("
8!");
				ssid_num_id = ssid_num_id - 1;
				f.wpa_key.focus();
				f.wpa_key.select();
				return;
			}
			else if(/.*[^\x00-\xff]+.*$/.test(f.wpa_key.value))
			{
				alert("
J!");
				f.wpa_key.select();
				ssid_num_id = ssid_num_id - 1;
				return;
			}
			else if (f.wpa_key.value.length == 64 && !isHex(f.wpa_key.value))
			{
				ssid_num_id = ssid_num_id - 1;
				alert("16
(0-9,a-f,A-F)
J!");
				f.wpa_key.focus();
				f.wpa_key.select();
				return;
			}
			setCfg("WPAPSK3Temp",f.wpa_key.value);
	//alert("securitymode::"+f.SecurityMode2.value);
	add_edit = 0;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','DHCP',cPage);
	return true;
function Editssid2(ssid_index)
	var ssid_num;
	var f=document.wireless_basic;
	var hide_ssid2_temp;
	var ssid2_auth_mode;
	var ssid2_encry_type;
	var ssid2_secu_mode;
	var ssid2_key1type ;
	var ssid2_key2type ;
	var ssid2_key3type ;
	var ssid2_key4type;
	var ssid2_key1_str2 ;
	var ssid2_key1_str3 ;
	var ssid2_key2_str2 ;
	var ssid2_key2_str3 ;
	var ssid2_key3_str2 ;
	var ssid2_key3_str3;
	var ssid2_key4_str2 ;
	var ssid2_key4_str3 ;
	var ssid2_wep_key_len ;
	var ssid2_wep_defau_key ;
	var ssid2_wpa_key2 ;
	if(ssid_index == 2)
		document.getElementById("app_btn").value = "
SSID";
		document.getElementById("app_btn").style.width = "120px";
		add_edit  = 2;
		ssid_num = 1 * bra_ssid_num;
		var tmpbuf=ssid_s2;
		if(ssid_s2.indexOf('&nbsp;')!=-1){
		for(var j=0;j<ssid_s2.length;j++)
			tmpbuf=tmpbuf.replace('&nbsp;',' ');
		f.ssid.value=tmpbuf;
					
		hide_ssid2_temp = broadcastssidEnable.split(";")[1];
		if(hide_ssid2_temp == 0)
			document.wireless_basic.broadcastssid[0].checked = true;
		else
			document.wireless_basic.broadcastssid[1].checked = true;
		ssid2_auth_mode =  auth_mode.split(";")[1];
		ssid2_encry_type =  encry_type.split(";")[1];
		ssid2_secu_mode =  secu_mode.split(";")[1];
		ssid2_key1type =  1 * key1type.split(";")[1];
		ssid2_key2type =  1 * key2type.split(";")[1];
		ssid2_key3type =  1 * key3type.split(";")[1];
		ssid2_key4type =  1 * key4type.split(";")[1];
		ssid2_key1_str2 =  key1_str2;
		ssid2_key1_str3 =  key1_str3;
		ssid2_key2_str2 =  key2_str2;
		ssid2_key2_str3 =  key2_str3;
		ssid2_key3_str2 =  key3_str2;
		ssid2_key3_str3 =  key3_str3;
		ssid2_key4_str2 =  key4_str2;
		ssid2_key4_str3 =  key4_str3;
	//alert("ssid2_key3_str2:::"+ssid2_key3_str2);
		ssid2_wep_key_len =  1 * wep_key_len.split(";")[1];
		ssid2_wep_defau_key = 1 * wep_defau_key.split(";")[1];
		ssid2_wpa_key2 =  wpa_key2;
		ssid2_secu_mode = 1 * ssid2_secu_mode;
	else if(ssid_index == 3)
		document.getElementById("app_btn").value = "
SSID";
		document.getElementById("app_btn").style.width = "120px";
		add_edit  = 3;
		ssid_num = 1 * bra_ssid_num;
		var tmpbuf=ssid_s3;
		if(ssid_s3.indexOf('&nbsp;')!=-1){
		for(var j=0;j<ssid_s3.length;j++)
			tmpbuf=tmpbuf.replace('&nbsp;',' ');
		f.ssid.value=tmpbuf;
		hide_ssid2_temp = broadcastssidEnable.split(";")[2];
		if(hide_ssid2_temp == 0)
			document.wireless_basic.broadcastssid[0].checked = true;
		else
			document.wireless_basic.broadcastssid[1].checked = true;
		ssid2_auth_mode =  auth_mode.split(";")[2];
		ssid2_encry_type =  encry_type.split(";")[2];
		ssid2_secu_mode =  secu_mode.split(";")[2];
		ssid2_key1type =  1 * key1type.split(";")[2];
		ssid2_key2type =  1 * key2type.split(";")[2];
		ssid2_key3type =  1 * key3type.split(";")[2];
		ssid2_key4type =  1 * key4type.split(";")[2];
		ssid2_key1_str2 =  key1_str2;
		ssid2_key1_str3 =  key1_str3;
		ssid2_key2_str2 =  key2_str2;
		ssid2_key2_str3 =  key2_str3;
		ssid2_key3_str2 =  key3_str2;
		ssid2_key3_str3 =  key3_str3;
		ssid2_key4_str2 =  key4_str2;
		ssid2_key4_str3 =  key4_str3;
			//alert("ssid2_key3_str2:::"+ssid2_key3_str2);
		ssid2_wep_key_len =  1 * wep_key_len.split(";")[2];
		ssid2_wep_defau_key = 1 * wep_defau_key.split(";")[2];
		ssid2_wpa_key2 =  wpa_key3;
		ssid2_secu_mode = 1 * ssid2_secu_mode;
	if(ssid2_secu_mode == 0)
		f.authen_code_sele.selectedIndex = 0;
		document.getElementById("encry_dis").checked = true;
		auth_sele_change();
	else if(ssid2_secu_mode == 1)
		if(ssid2_auth_mode == "OPEN")
			f.authen_code_sele.selectedIndex = 1;
		else
			f.authen_code_sele.selectedIndex = 2;
		auth_sele_change();
		if(ssid2_wep_key_len == 0)//wep64
			document.getElementById("encry_wep64").checked = true;
		else//wep128
			document.getElementById("encry_wep128").checked = true;
		if(ssid2_wep_defau_key == 1)
			f.wep_key_sele[0].checked = true;
			if(ssid_index == 2)
			{
				var tmpbuf=ssid2_key1_str2;
				if(ssid2_key1_str2.indexOf('&quot;')!=-1){
				for(var j=0;j<ssid2_key1_str2.length;j++)
					tmpbuf=tmpbuf.replace('&quot;','"');
				}
					f.wep5_key1.value=tmpbuf;
			}
			else
