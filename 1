function trim(str){ //?
return str.replace(/(^\s*)|(\s*$)/g, ""); 
function acl_CheckMAC(mac_addr)
	var f=document.wireless_security;
	var	i, idx,num=0;
	var mac_tmp=mac_addr.split(":");
	var mac_addr2="";
	for(i=0;i<mac_tmp.length;i++)
	  mac_addr2+=mac_tmp[i];
    if (f.editRow.value == "-1") num=count	+1;
    else num=f.editRow.value;
	if (num>max)
    	alert("
uMAC
I"); 
		return false;
    
	if(mac_addr2.length==0){
		alert("
uMAC
I");
		return false ;
	if ( mac_addr2.length != 12) {
		alert("
uMAC
I");
		return false;
  	}
  	if(mac_addr2 == "000000000000" || mac_addr2.toUpperCase() == "FFFFFFFFFFFF") {
		alert("
uMAC
G00:00:00:00:00:00 
 ff:ff:ff:ff:ff:ff
C");
		return false;
	if ( mac_addr2.length != 12) {
		alert("
uMAC
I");
		return false;
  	}
	if((mac_addr2.charAt(1)&0x01)||(mac_addr2.charAt(1).toUpperCase()=='B')||(mac_addr2.charAt(1).toUpperCase()=='D')||(mac_addr2.charAt(1).toUpperCase()=='F')){
		alert("
uMAC
C");
		return false;
	for(i = 0; i < 5; i++)
		if(isNaN(parseInt(mac_addr.charAt(3*i), 16)))
			alert('MAC
J.');
			return false;
		}	
		else if(isNaN(parseInt(mac_addr.charAt(3*i+1), 16)))
			alert('MAC
J.');
		    return false;
		}    
		else if (mac_addr.charAt(3*i+2) != ':')
			alert('MAC
J.');
			return false;     	
		}    
	if(isNaN(parseInt(mac_addr.charAt(15), 16)))
		alert('MAC
J.');
		return false;
	else if(isNaN(parseInt(mac_addr.charAt(16), 16)))
		alert('MAC
J.');
		return false;
	return true;
function apply()
	var f=document.wireless_security;
	if (f.acl_mode[0].checked == true)
		f.AccessPolicy0Temp.value = 0;
	else if(f.acl_mode[1].checked == true)
		f.AccessPolicy0Temp.value = 1;
	else if(f.acl_mode[2].checked == true)
		f.AccessPolicy0Temp.value = 2;
	f.SSIDIndexTemp.value = 0;
	f.AccessControlList0Temp.value = acl_mac_list1;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SECURITY',cPage);
	return true;
function Dele()
	var i,j;
	var f=document.wireless_security;
	var temp = "";
	var k;
	k = 0;
	j = 0;
	for(i=0 ;i < 14;i++)
		if(str.split(";")[i] !='' && typeof(str.split(";")[i])!= "undefined")
			j++;
	for(i=0 ;i < j;i++)
		if(j == 1)
			if(document.wireless_security.wz_acl.checked == false)
				temp += str.split(";")[i]; 
			else
				temp = "";
		else if(document.wireless_security.wz_acl[i].checked == false)
			if(k > 0)
				temp += ";";
			temp += str.split(";")[i];
			k++;
	f.AccessPolicy0Temp.value = acl_policy0;
	f.SSIDIndexTemp.value = 0;
	f.AccessControlList0Temp.value = temp;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SECURITY',cPage);
	return true;
function Add()
	var f=document.wireless_security;
	var ap_bssid=f.ap_bssid1.value+":"+f.ap_bssid2.value+":"+f.ap_bssid3.value+":"+f.ap_bssid4.value+":"+f.ap_bssid5.value+":"+f.ap_bssid6.value;
	if(!acl_CheckMAC(ap_bssid))
		f.ap_bssid1.value = "";
		f.ap_bssid2.value = "";
		f.ap_bssid3.value = "";
		f.ap_bssid4.value = "";
		f.ap_bssid5.value = "";
		f.ap_bssid6.value = "";
		f.ap_bssid1.focus();
		f.ap_bssid1.select();
		return;
	if(parseInt(trim(f.ap_bssid1.value),16)==255 && parseInt(trim(f.ap_bssid2.value),16)==255  && parseInt(trim(f.ap_bssid3.value),16)==255 && parseInt(trim(f.ap_bssid4.value),16)==255 && parseInt(trim(f.ap_bssid5.value),16)==255 && parseInt(trim(f.ap_bssid6.value),16)==255)
		alert("MAC
J!");
		return;
	if(parseInt(trim(f.ap_bssid1.value),16)==0 && parseInt(trim(f.ap_bssid2.value),16)==0  && parseInt(trim(f.ap_bssid3.value),16)==0 && parseInt(trim(f.ap_bssid4.value),16)==0 && parseInt(trim(f.ap_bssid5.value),16)==0 && parseInt(trim(f.ap_bssid6.value),16)==0)
		alert("MAC
J!");
		return;
	if((parseInt(trim(f.ap_bssid1.value),16)==1 && parseInt(trim(f.ap_bssid2.value),16)==0  && parseInt(trim(f.ap_bssid3.value),16)==94 && parseInt(trim(f.ap_bssid4.value),16)<128))
		alert("MAC
J!");
		return;
	if(acl_mac_list1 == '' || typeof(acl_mac_list1) == "undefined")
		f.AccessControlList0Temp.value = ap_bssid;
	else
		var str1 = ap_bssid;
		var str2 = acl_mac_list1;
		if(str2.indexOf(str1) >= 0)
			alert("MAC
b!") ;
			return;
		f.AccessControlList0Temp.value = acl_mac_list1 +";"+ ap_bssid;
	f.AccessPolicy0Temp.value = acl_policy0;
	f.SSIDIndexTemp.value = 0;
	form2Cfg(f);
	subForm(f,'do_cmd.htm','WIRELESS_SECURITY',cPage);
	return true;
function init()
	var f=document.wireless_security;
	if(acl_policy0 == 0)
		f.acl_mode[0].checked = true ;
	else if(acl_policy0 == 1)
		f.acl_mode[1].checked = true ;
	else if(acl_policy0 == 2)
		f.acl_mode[2].checked = true ;
