	else
		str += "<td class=item_td colspan=2>" + ssid_s1.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+ auth_mode.split(";")[0] + "-" + encry_type.split(";")[0] + ")</font></td>";
	str += "</tr>";
	str1 += "<tr><td class=item_td><img src='images/radio.gif'></td>";
	//str1 += ssid_s2+"<br>";
	if(secu_mode.split(";")[1] == 0 )
		str1 += "<td class=item_td >"+ssid_s2.replace(eval("/ /gi"),'&nbsp;')+"</td>";
	else
		str1 += "<td class=item_td >"  + ssid_s2.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+auth_mode.split(";")[1]+"-"+encry_type.split(";")[1]+")</font></td>";
	str1 += "<td class=item_td align=right ><input type=checkbox name=ssid2check  value=ssid2>&nbsp;&nbsp;&nbsp;&nbsp;";
	str1 += "<input type=button style=\"{width:70px;}\" value=
 name=Edit2 class=p_font onclick=Editssid2(2)></td>";
	str1 += "</tr>";
	str2 += "<tr><td class=item_td><img src='images/radio.gif'></td>";
	//str2 += ssid_s3+"<br>";
	if(secu_mode.split(";")[2] == 0 )
		str2 += "<td class=item_td >" + ssid_s3.replace(eval("/ /gi"),'&nbsp;') + "</td>";
	else if(secu_mode.split(";")[2] == 1 || secu_mode.split(";")[2] == 3)
		str2 += "<td class=item_td >"+ ssid_s3.replace(eval("/ /gi"),'&nbsp;') +"<br><font color=gray>("+auth_mode.split(";")[2]+"-"+encry_type.split(";")[2]+")</font></td>";
	str2 += "<td class=item_td align=right ><input type=checkbox name=ssid3check  value=ssid3>&nbsp;&nbsp;&nbsp;&nbsp;";
	str2 += "<input type=button style=\"{width:70px;}\" value=
 name=Edit3 class=p_font onclick=Editssid2(3)></td>";
	str2 += "</tr>";
	document.write(str);
	document.write(str1);
	document.write(str2);
</script>
</table>
<br>
<br>
</form>
</body>
