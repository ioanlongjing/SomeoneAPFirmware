function genTimeOpt()
	var s='';
	for (i=0; i<timeTable.length; i++)
		var t=timeTable[i];
		s+='<option value='+t[0]+'> (GMT'+t[1]+') '+t[2]+'</option>\n';
	document.write(s);
function setCln(f,m){
	f.clnEn.value=1;
	decomMAC2(f.WMAC, m, 1);
	f.cln.value='
]MAC
function setCln1(f,m){
	f.clnEn_1.value=1;
	decomMAC2(f.WMAC_1, m, 1);
	f.cln1.value='
]MAC
function clrCln(f){
	f.clnEn.value=0;
	decomMAC2(f.WMAC, '', 1);
	f.cln.value='
sMAC
function clrCln1(f){
	f.clnEn_1.value=0;
	decomMAC2(f.WMAC_1, '', 1);
	f.cln1.value='
sMAC
function clnMac(f)
	if  (f.clnEn.value=='1') clrCln(f); else setCln(f,cln_MAC);
function cln1Mac(f)
	if  (f.clnEn_1.value=='1') clrCln1(f); else setCln1(f,cln_MAC);
function evalDnsFix(ds1,ds2)
	if ((ds1==''||ds1=='0.0.0.0')&&(ds2==''||ds2=='0.0.0.0')) return 0;
	return 1;
function pagetitle(f)
	var str;
	var ie=navigator.appName == "Microsoft Internet Explorer" ? true : false; 
	str ='<table width=700 border=0 >';
	str +='<tr >';
	str +='<td width=20>';
	str +='<img src=\"images/TITLE_ICON.png\"> </td><td width=680 class=web_title1>';
	str += f;
	str +='</td>';
	str +='</tr>';
	str +='</table>\
		  ';
	if(!ie)
		str +='<br>';
	document.write(str);
