	var str = "";
	var str1= ""; 
	//var str = "1234";
	//var str1= "CCK;ofdm;htmix;CCK"; 
	var total_number=4;
	var i,total_wds=0;
	var wds_mac = new Array();
	//document.write(str.charAt(0)+":"+str.charAt(1));
	if(str == '')
		total_wds=0;
	else 
		for(i=0;i<total_number;i++)
			wds_mac[i] = str.split(";")[i];
			if(str.split(";")[i] !='' && typeof(str.split(";")[i])!= "undefined")
			{
				total_wds++;
				//alert(str.split(";")[i]));
				document.write("<tr><td class=item_td align=center>"+str.split(";")[i]+"</td><td class=item_td >&nbsp;</td><td class=item_td align=center><input type=checkbox name=wdsmac ></td></tr>");
			}
