			document.getElementById("div_11n_channel_china").style.display = "none";
var hide_ssid_temp = broadcastssidEnable.split(";")[0];
var key1type_tm = 1 * key1type.split(";")[0];//0 hex 1 ascii
var key2type_tm = 1 *  key2type.split(";")[0];
var key3type_tm = 1 *  key3type.split(";")[0];
var key4type_tm = 1 *  key4type.split(";")[0];
var sec_mode = 1 *  secu_mode.split(";")[0];
//alert(sec_mode);
var authtype_temp = auth_mode.split(";")[0];
//alert(authtype_temp);
var keyid_temp = 1 *  wep_defau_key.split(";")[0];	
var keylen_temp = 1 *  wep_key_len.split(";")[0];	
var encry_type_temp = encry_type.split(";")[0];
function init()
	f=document.wireless_basic;
	cfg2Form(f);
	document.getElementById("wzssid").value = ssid_s1.replace('&quot;','"');
	if(hide_ssid_temp == 0)
		document.wireless_basic.broadcastssid[0].checked = true;
	else
		document.wireless_basic.broadcastssid[1].checked = true;
	if(ht_bw == 1)
			document.wireless_basic.bandwidth_ra[1].checked = true;
	else
		document.wireless_basic.bandwidth_ra[0].checked = true;
	if(countrycode == "TW")
		document.getElementById("country_code_sele").selectedIndex = 3;
	else if(countrycode == "CN")
		document.getElementById("country_code_sele").selectedIndex = 1;
	else if(countrycode == "JP")
		document.getElementById("country_code_sele").selectedIndex = 2;
	else if(countrycode == "US")
		document.getElementById("country_code_sele").selectedIndex = 0;
	if((PhyMode == 0) || (PhyMode == 1))
		document.getElementById("wirelessmode").selectedIndex = PhyMode;
	else if(PhyMode == 4)
		document.getElementById("wirelessmode").selectedIndex = 2;
	else if(PhyMode == 9)
		document.getElementById("wirelessmode").selectedIndex = 3;
	wirelessModeChange();
	channel_index = 1 * channel_index;
	if((PhyMode == 0) || (PhyMode == 1) || (PhyMode == 4) || (ht_bw == 0))
		document.getElementById("channel_sele_usa").selectedIndex =  channel_index - 1;
		document.getElementById("channel_sele_china").selectedIndex = channel_index - 1;
		document.getElementById("channel_sele_japan").selectedIndex = channel_index - 1;
	else if(PhyMode == 9)
		if(channel_index < 5)
			document.getElementById("channel_sele_usa_n").selectedIndex = channel_index - 1;
			document.getElementById("channel_sele_china_n").selectedIndex = channel_index - 1;
		else if(channel_index < 10)
			//alert(channel_index+"<10");
			var channel_tm;
			if(channel_index == 5)
				if(ht_extcha == 0)//upper
					channel_tm = 6;
				else//lower
					channel_tm = 5;
			else if(channel_index == 6)
				if(ht_extcha == 0)//upper
					channel_tm = 8;
				else//lower
					channel_tm = 7;
			else if(channel_index == 7)
				if(ht_extcha == 0)//upper
					channel_tm = 10;
				else//lower
					channel_tm = 9;
			else if(channel_index == 8 && (countrycode == "TW" || countrycode == "US"))
					channel_tm = 11;
			else if(channel_index == 8 )
				if(ht_extcha == 0)//upper
					channel_tm = 12;
				else//lower
					channel_tm = 11;
			else if(channel_index == 9 && (countrycode == "TW" || countrycode == "US"))
					channel_tm = 12;
			else if(channel_index == 9 )
				if(ht_extcha == 0)//upper
					channel_tm = 14;
				else//lower
					channel_tm = 13;
			document.getElementById("channel_sele_usa_n").selectedIndex = channel_tm - 1;
			document.getElementById("channel_sele_china_n").selectedIndex = channel_tm - 1;
		else if(channel_index < 14)
			//alert(channel_index+"<14");
			temp = 5 + channel_index;
			//alert(countrycode);
			if(countrycode == "US" || countrycode == "TW")
			{	
				//alert(countrycode);
				temp = 3 + channel_index;
				document.getElementById("channel_sele_usa_n").selectedIndex = temp - 1;
			}
			else
				document.getElementById("channel_sele_china_n").selectedIndex = temp - 1;
				
	//alert("debug 1");
	if(sec_mode == 0)
		if(wsc_auth > 30)
				var tmpbuf=wpa_key;
				if(wpa_key.indexOf('&quot;')!=-1){
				for(var j=0;j<wpa_key.length;j++)
					tmpbuf=tmpbuf.replace('&quot;','"');
				}
				document.wireless_basic.wpa_key.value=tmpbuf;
			if(authtype_temp == "WPAPSK")//wpa
			{
				document.wireless_basic.authen_code_sele.selectedIndex = 3;
				auth_sele_change();
				if(encry_type_temp == "TKIP")
					document.wireless_basic.encry_type[3].checked = true;
				else if(encry_type_temp == "AES")
					document.wireless_basic.encry_type[4].checked = true;
			}
			else if(authtype_temp == "WPA2PSK")//wpa2
			{
				//alert("WPA2PSK");
				document.wireless_basic.authen_code_sele.selectedIndex = 4;
				auth_sele_change();
				if(encry_type_temp == "TKIP")
				{
					//alert("TKIP");
					document.wireless_basic.encry_type[3].checked = true;
				}
				else if(encry_type_temp == "AES")
				{
					//alert("AES");
					document.wireless_basic.encry_type[4].checked = true;
				}
				else if(encry_type_temp == "TKIPAES")
				{
					//alert("TKIPAES");
					document.wireless_basic.encry_type[5].checked = true;
				}
			}
			else if(authtype_temp == "WPAPSKWPA2PSK")//wpa_wpa2
			{
				document.wireless_basic.authen_code_sele.selectedIndex = 5;
				auth_sele_change();
				if(encry_type_temp == "TKIP")
					document.wireless_basic.encry_type[3].checked = true;
				else if(encry_type_temp == "AES")
					document.wireless_basic.encry_type[4].checked = true;
				else if(encry_type_temp == "TKIPAES")
					document.wireless_basic.encry_type[5].checked = true;
			}
		else
			document.wireless_basic.authen_code_sele.selectedIndex = 0;
			auth_sele_change();
	else if(sec_mode == 1)
		if(authtype_temp == "OPEN")//open
			document.wireless_basic.authen_code_sele.selectedIndex = 1;
		else if(authtype_temp == "SHARED")//shared key
			document.wireless_basic.authen_code_sele.selectedIndex = 2;
		auth_sele_change();
		//key len 0 --64  1--128
		if(keylen_temp == 0)
			document.getElementById("encry_wep64").checked = true;
		else
			document.getElementById("encry_wep128").checked = true;
		//key index
		//key meth ---0 hex 1 ascii
		//wep key
		if(keyid_temp == 1)
			document.wireless_basic.wep_key_sele[0].checked = true;
			var tmpbuf=key1_str;
				if(key1_str.indexOf('&quot;')!=-1){
				for(var j=0;j<key1_str.length;j++)
					tmpbuf=tmpbuf.replace('&quot;','"');
				}
			document.getElementById("wep5_key").value = tmpbuf;
			if(key1type_tm == 1)
			{
				document.wireless_basic.wep_key_meth[0].checked = true;//ascii
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 5;	
					document.getElementById("wep5_key").size = 5;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 13;	
					document.getElementById("wep5_key").size = 13;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
				}
				
			}
			else
			{
				document.wireless_basic.wep_key_meth[1].checked = true;//hex
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 10;	
					document.getElementById("wep5_key").size = 10;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 26;	
					document.getElementById("wep5_key").size = 32;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
				}
			}
		else if(keyid_temp == 2)
			document.wireless_basic.wep_key_sele[1].checked = true;
			var tmpbuf=key2_str;
			if(key2_str.indexOf('&quot;')!=-1){
			for(var j=0;j<key2_str.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.getElementById("wep5_key").value = tmpbuf;
			if(key2type_tm == 1)
			{
				document.wireless_basic.wep_key_meth[0].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 5;	
					document.getElementById("wep5_key").size = 5;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 13;	
					document.getElementById("wep5_key").size = 13;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
				}
			}
			else
			{
				document.wireless_basic.wep_key_meth[1].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 10;	
					document.getElementById("wep5_key").size = 10;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 26;	
					document.getElementById("wep5_key").size = 32;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
				}
			}
		else if(keyid_temp == 3)
			document.wireless_basic.wep_key_sele[2].checked = true;
			var tmpbuf=key3_str;
			if(key3_str.indexOf('&quot;')!=-1){
			for(var j=0;j<key3_str.length;j++)
			tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.getElementById("wep5_key").value = tmpbuf;
			if(key3type_tm == 1)
			{
				document.wireless_basic.wep_key_meth[0].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 5;	
					document.getElementById("wep5_key").size = 5;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 13;	
					document.getElementById("wep5_key").size = 13;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
				}
			}
			else
			{
				document.wireless_basic.wep_key_meth[1].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 10;	
					document.getElementById("wep5_key").size = 10;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 26;	
					document.getElementById("wep5_key").size = 32;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
				}
			}
		else
			document.wireless_basic.wep_key_sele[3].checked = true;
			var tmpbuf=key4_str;
			if(key4_str.indexOf('&quot;')!=-1){
			for(var j=0;j<key4_str.length;j++)
				tmpbuf=tmpbuf.replace('&quot;','"');
			}
			document.getElementById("wep5_key").value = tmpbuf;
			if(key4type_tm == 1)
			{
				document.wireless_basic.wep_key_meth[0].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 5;	
					document.getElementById("wep5_key").size = 5;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=5)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 13;	
					document.getElementById("wep5_key").size = 13;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=13)";
				}
			}
			else
			{
				document.wireless_basic.wep_key_meth[1].checked = true;
				if(keylen_temp == 0)//64
				{
					document.getElementById("wep5_key").maxLength = 10;	
					document.getElementById("wep5_key").size = 10;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=10)";
				}
				else
				{
					document.getElementById("wep5_key").maxLength = 26;	
					document.getElementById("wep5_key").size = 32;
					document.getElementById("wep_key_length").innerHTML = "
X<br>(
=26)";
				}
			}
	else if(sec_mode == 3)
		var tmpbuf=wpa_key;
		if(wpa_key.indexOf('&quot;')!=-1){
		for(var j=0;j<wpa_key.length;j++)
			tmpbuf=tmpbuf.replace('&quot;','"');
		document.wireless_basic.wpa_key.value=tmpbuf;
		if(authtype_temp == "WPAPSK")//wpa
			document.wireless_basic.authen_code_sele.selectedIndex = 3;
			auth_sele_change();
			if(encry_type_temp == "TKIP")
				document.wireless_basic.encry_type[3].checked = true;
			else if(encry_type_temp == "AES")
				document.wireless_basic.encry_type[4].checked = true;
		else if(authtype_temp == "WPA2PSK")//wpa2
			//alert("WPA2PSK");
			document.wireless_basic.authen_code_sele.selectedIndex = 4;
			auth_sele_change();
			if(encry_type_temp == "TKIP")
			{
				//alert("TKIP");
				document.wireless_basic.encry_type[3].checked = true;
			}
			else if(encry_type_temp == "AES")
			{
				//alert("AES");
				document.wireless_basic.encry_type[4].checked = true;
			}
			else if(encry_type_temp == "TKIPAES")
			{
				//alert("TKIPAES");
				document.wireless_basic.encry_type[5].checked = true;
			}
		else if(authtype_temp == "WPAPSKWPA2PSK")//wpa_wpa2
			document.wireless_basic.authen_code_sele.selectedIndex = 5;
			auth_sele_change();
			if(encry_type_temp == "TKIP")
				document.wireless_basic.encry_type[3].checked = true;
			else if(encry_type_temp == "AES")
				document.wireless_basic.encry_type[4].checked = true;
			else if(encry_type_temp == "TKIPAES")
				document.wireless_basic.encry_type[5].checked = true;
function apply()
	var WepKeyLength2_st;
	var WepDefaultKey2_st;
	var Key1TypeTemp_st;
	var Key2TypeTemp_st;
	var Key3TypeTemp_st;
	var Key4TypeTemp_st;
	var Key1Str1Temp_st;
	var Key1Str2Temp_st;
	var Key1Str3Temp_st;
	var Key1Str4Temp_st;
	var WPAPSK1Temp_st;
	var f=document.wireless_basic;
	if(f.ssid.value == '')
		alert("SSID
J!");
		return;
	else if(/.*[^\x00-\xff]+.*$/.test(f.ssid.value))
		alert("SSID
J!");
		f.ssid.select();
		return;
	/*else if ((f.ssid.value.indexOf("'")!=-1)||(f.ssid.value.indexOf('"')!=-1)||
		   (f.ssid.value.indexOf("#")!=-1)||(f.ssid.value.indexOf("!")!=-1)||
		   (f.ssid.value.indexOf("^")!=-1)||(f.ssid.value.indexOf("|")!=-1))
			alert("SSID
[' \" # ! ^ |],
J!");
			f.ssid.select();
			return;
	}*/
	//WirelessMode2
	if(f.wirelessmode.options.selectedIndex == 0)
		f.WirelessMode2.value = 0;
	else if(f.wirelessmode.options.selectedIndex == 1)
		f.WirelessMode2.value = 1;
	else if(f.wirelessmode.options.selectedIndex == 2)
		f.WirelessMode2.value = 4;
	else if(f.wirelessmode.options.selectedIndex == 3)
		f.WirelessMode2.value = 9;
	//HideSSID2
	if(f.broadcastssid[0].checked == true)
		f.HideSSID2.value = "0;"+broadcastssidEnable.split(";")[1]+";"+broadcastssidEnable.split(";")[2]+";";
	else
		f.HideSSID2.value = "1;"+broadcastssidEnable.split(";")[1]+";"+broadcastssidEnable.split(";")[2]+";";
	//country_code
	if(f.country_code_sele.options.selectedIndex == 0)
		f.country_code.value = "US";
	else if(f.country_code_sele.options.selectedIndex == 1)
		f.country_code.value = "CN";
	else if(f.country_code_sele.options.selectedIndex == 2)
		f.country_code.value = "JP";
	else if(f.country_code_sele.options.selectedIndex == 3)
		f.country_code.value = "TW";
	//sz11gChannel
	var phy_mod = f.wirelessmode.options.selectedIndex;
	var code = f.country_code_sele.options.selectedIndex;
	var channel_tmp;
	var ext_ch;
	phy_mod = 1 * phy_mod;
	//alert("debug................"+phy_mod);
	if(phy_mod == 0 || phy_mod == 1 || phy_mod == 2 || f.bandwidth_ra[0].checked == true)
		if(code == 0 || code == 3)
			channel_tmp = f.channel_sele_usa.options.selectedIndex + 1;
		else if(code == 1)
			channel_tmp = f.channel_sele_china.options.selectedIndex + 1;
		else if(code == 2)
			channel_tmp = f.channel_sele_japan.options.selectedIndex + 1;
		ext_ch = 1;
	else if(phy_mod == 3)
		//alert("phy_mod:...........9");
		if(code == 0 || code == 3)
			channel_tmp = f.channel_sele_usa_n.options.selectedIndex + 1;
		else 
			channel_tmp = f.channel_sele_china_n.options.selectedIndex + 1;
		//alert("debug.........."+channel_tmp);
		if(channel_tmp < 5)
			channel_tmp = channel_tmp;
			ext_ch = 1;
		else if(channel_tmp < 15)
			if(channel_tmp == 5)
			{
				channel_tmp = 5;
				ext_ch = 1;
			}
			else if(channel_tmp == 6)
			{
				channel_tmp = 5;
				ext_ch = 0;
			}
			else if(channel_tmp == 7)
			{
				channel_tmp = 6;
				ext_ch = 1;
			}
			else if(channel_tmp == 8)
			{
				channel_tmp = 6;
				ext_ch = 0;
			}
			else if(channel_tmp == 9)
			{
				channel_tmp = 7;
				ext_ch = 1;
			}
			else if(channel_tmp == 10)
			{
				channel_tmp = 7;
				ext_ch = 0;
			}
			else if(channel_tmp == 11 && (code == 0 || code == 3))
			{
				channel_tmp = 8;
				ext_ch = 0;
			}
			else if(channel_tmp == 11)
			{
				channel_tmp = 8;
				ext_ch = 1;
			}
			else if(channel_tmp == 12 && (code == 0 || code == 3))
			{
				channel_tmp = 9;
				ext_ch = 0;
			}
			else if(channel_tmp == 12)
			{
				channel_tmp = 8;
				ext_ch = 0;
			}
			else if(channel_tmp == 13 && (code == 0 || code == 3))
			{
				channel_tmp = 10;
				ext_ch = 0;
			}
			else if(channel_tmp == 13)
			{
				channel_tmp = 9;
				ext_ch = 1;
			}
			else if(channel_tmp == 14 && (code == 0 || code == 3))
			{
				channel_tmp = 11;
				ext_ch = 0;
			}
			else if(channel_tmp == 14)	
			{
				channel_tmp = 9;
				ext_ch = 0;
			}
		else if(channel_tmp < 19)
			//alert("debug......channel_tmp < 19");
			channel_tmp = channel_tmp - 5;
			ext_ch = 0;
	f.sz11gChannel.value = channel_tmp;	
	//n_bandwidth
	if(f.bandwidth_ra[0].checked == true)
		f.n_bandwidth.value = 0;
	else
		f.n_bandwidth.value = 1;
	//EXTCHA2
	f.EXTCHA2.value = ext_ch;
	//AutoChannelSelect2
	if( channel_tmp == 0)
		f.AutoChannelSelect2.value = 2;
	else 
		f.AutoChannelSelect2.value = 0;
	//WscAuthType2
	//WscEncrypType2
	//SecurityMode2
	//AuthMode
	//EncrypType
	//WepKeyLength2 0--->64 1--->128
	//WepDefaultKey2
	//Key1TypeTemp
	//Key2TypeTemp
	//Key3TypeTemp
	//Key4TypeTemp
	//Key1Str1Temp
	//Key2Str1Temp
	//Key3Str1Temp
	//Key4Str1Temp
	//WPAPSK1Temp
	if(f.authen_code_sele.options.selectedIndex == 0)
		f.WscAuthType2.value = 1;
		f.WscEncrypType2.value = 1;
		f.SecurityMode2.value = "0;"+secu_mode.split(";")[1]+";"+secu_mode.split(";")[2];
		f.AuthMode.value ="OPEN;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
		f.EncrypType.value = "NONE;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
	else if(f.authen_code_sele.options.selectedIndex == 1 || f.authen_code_sele.options.selectedIndex == 2)
		f.WscAuthType2.value = 1;
		f.WscEncrypType2.value = 2;
		if(f.authen_code_sele.options.selectedIndex == 1)
			f.SecurityMode2.value = "1;"+secu_mode.split(";")[1]+";"+secu_mode.split(";")[2];
			f.AuthMode.value ="OPEN;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
			f.EncrypType.value = "WEP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
		else
			f.SecurityMode2.value = "1;"+secu_mode.split(";")[1]+";"+secu_mode.split(";")[2];
			f.AuthMode.value ="SHARED;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
			f.EncrypType.value = "WEP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
		if(f.encry_type[1].checked == true)//wep64
			WepKeyLength2_st = "0;"+wep_key_len.split(";")[1]+";"+wep_key_len.split(";")[2];
			setCfg("WepKeyLength2",WepKeyLength2_st);
			if(f.wep_key_meth[0].checked == true)//ascii 5
			{
				if(f.wep5_key1.value.length != 5)
				{
					alert("
5!");
					return;
				}
				else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
				{
					alert("
J!");
					f.wep5_key1.select();
					return;
				}
					
				if(f.wep_key_sele[0].checked == true)//key1
				{
					WepDefaultKey2_st = "1;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key1TypeTemp_st = "1;"+key1type.split(";")[1]+";"+key1type.split(";")[2];
					Key1Str1Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key1TypeTemp",Key1TypeTemp_st);
					setCfg("Key1Str1Temp",Key1Str1Temp_st);
				}
				else if(f.wep_key_sele[1].checked == true)//key2
				{
					WepDefaultKey2_st = "2;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key2TypeTemp_st = "1;"+key2type.split(";")[1]+";"+key2type.split(";")[2];
					Key1Str2Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key2TypeTemp",Key2TypeTemp_st);
					setCfg("Key2Str1Temp",Key1Str2Temp_st);
				}
				else if(f.wep_key_sele[2].checked == true)//key3
				{
					WepDefaultKey2_st = "3;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key3TypeTemp_st = "1;"+key3type.split(";")[1]+";"+key3type.split(";")[2];
					Key1Str3Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key3TypeTemp",Key3TypeTemp_st);
					setCfg("Key3Str1Temp",Key1Str3Temp_st);
				}
				else//key4		
				{
					WepDefaultKey2_st = "4;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key4TypeTemp_st = "1;"+key4type.split(";")[1]+";"+key4type.split(";")[2];
					Key1Str4Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key4TypeTemp",Key4TypeTemp_st);
					setCfg("Key4Str1Temp",Key1Str4Temp_st);
				}
			}
			else//hex 10
			{
				if(f.wep5_key1.value.length != 10)
				{
					alert("
10!");
					return;
				}
				if(IsHex(f.wep5_key1.value))
				{
					alert("16
(0-9,a-f,A-F)
J!");
					return;
				}
				if(f.wep_key_sele[0].checked == true)//key1
				{
					//alert("key1_10");
					WepDefaultKey2_st = "1;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key1TypeTemp_st = "0;"+key1type.split(";")[1]+";"+key1type.split(";")[2];
					Key1Str1Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key1TypeTemp",Key1TypeTemp_st);
					setCfg("Key1Str1Temp",Key1Str1Temp_st);
				}
				else if(f.wep_key_sele[1].checked == true)//key2
				{
					WepDefaultKey2_st = "2;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key2TypeTemp_st = "0;"+key2type.split(";")[1]+";"+key2type.split(";")[2];
					Key1Str2Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key2TypeTemp",Key2TypeTemp_st);
					setCfg("Key2Str1Temp",Key1Str2Temp_st);
				}
				else if(f.wep_key_sele[2].checked == true)//key3
				{
					WepDefaultKey2_st = "3;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key3TypeTemp_st = "0;"+key3type.split(";")[1]+";"+key3type.split(";")[2];
					Key1Str3Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key3TypeTemp",Key3TypeTemp_st);
					setCfg("Key3Str1Temp",Key1Str3Temp_st);
				}
				else//key4		
				{
					WepDefaultKey2_st = "4;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key4TypeTemp_st = "0;"+key4type.split(";")[1]+";"+key4type.split(";")[2];
					Key1Str4Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key4TypeTemp",Key4TypeTemp_st);
					setCfg("Key4Str1Temp",Key1Str4Temp_st);
				}
				
			}
				
		else if(f.encry_type[2].checked == true)
			WepKeyLength2_st = "1;"+wep_key_len.split(";")[1]+";"+wep_key_len.split(";")[2];
			setCfg("WepKeyLength2",WepKeyLength2_st);
			if(f.wep_key_meth[0].checked == true)//ascii 13
			{
				if(f.wep5_key1.value.length != 13)
				{
					alert("
13!");
					return;
				}
				else if(/.*[^\x00-\xff]+.*$/.test(f.wep5_key1.value))
				{
					alert("
J!");
					f.wep5_key1.select();
					return;
				}
				if(f.wep_key_sele[0].checked == true)//key1
				{
					WepDefaultKey2_st = "1;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key1TypeTemp_st = "1;"+key1type.split(";")[1]+";"+key1type.split(";")[2];
					Key1Str1Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key1TypeTemp",Key1TypeTemp_st);
					setCfg("Key1Str1Temp",Key1Str1Temp_st);
				}
				else if(f.wep_key_sele[1].checked == true)//key2
				{
					WepDefaultKey2_st = "2;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key2TypeTemp_st = "1;"+key2type.split(";")[1]+";"+key2type.split(";")[2];
					Key1Str2Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key2TypeTemp",Key2TypeTemp_st);
					setCfg("Key2Str1Temp",Key1Str2Temp_st);
				}
				else if(f.wep_key_sele[2].checked == true)//key3
				{
					WepDefaultKey2_st = "3;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key3TypeTemp_st = "1;"+key3type.split(";")[1]+";"+key3type.split(";")[2];
					Key1Str3Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key3TypeTemp",Key3TypeTemp_st);
					setCfg("Key3Str1Temp",Key1Str3Temp_st);
				}
				else//key4		
				{
					WepDefaultKey2_st = "4;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key4TypeTemp_st = "1;"+key4type.split(";")[1]+";"+key4type.split(";")[2];
					Key1Str4Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key4TypeTemp",Key4TypeTemp_st);
					setCfg("Key4Str1Temp",Key1Str4Temp_st);
				}
				
			}
			else//hex 26
			{
				if(f.wep5_key1.value.length != 26)
				{
					alert("
26!");
					return;
				}
				if(IsHex(f.wep5_key1.value))
				{
					alert("16
(0-9,a-f,A-F)
J!");
					return;
				}
				if(f.wep_key_sele[0].checked == true)//key1
				{
					WepDefaultKey2_st = "1;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key1TypeTemp_st = "0;"+key1type.split(";")[1]+";"+key1type.split(";")[2];
					Key1Str1Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key1TypeTemp",Key1TypeTemp_st);
					setCfg("Key1Str1Temp",Key1Str1Temp_st);
				}
				else if(f.wep_key_sele[1].checked == true)//key2
				{
					WepDefaultKey2_st = "2;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key2TypeTemp_st = "0;"+key2type.split(";")[1]+";"+key2type.split(";")[2];
					Key1Str2Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key2TypeTemp",Key2TypeTemp_st);
					setCfg("Key2Str1Temp",Key1Str2Temp_st);
				}
				else if(f.wep_key_sele[2].checked == true)//key3
				{
					WepDefaultKey2_st = "3;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key3TypeTemp_st = "0;"+key3type.split(";")[1]+";"+key3type.split(";")[2];
					Key1Str3Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key3TypeTemp",Key3TypeTemp_st);
					setCfg("Key3Str1Temp",Key1Str3Temp_st);
				}
				else//key4		
				{
					WepDefaultKey2_st = "4;"+wep_defau_key.split(";")[1]+";"+wep_defau_key.split(";")[2];
					Key4TypeTemp_st = "0;"+key4type.split(";")[1]+";"+key4type.split(";")[2];
					Key1Str4Temp_st = f.wep5_key1.value ;
					setCfg("WepDefaultKey2",WepDefaultKey2_st);
					setCfg("Key4TypeTemp",Key4TypeTemp_st);
					setCfg("Key4Str1Temp",Key1Str4Temp_st);
				}
			}
							
	else 
		f.WscAuthType2.value = 2;
		f.SecurityMode2.value = "3;"+secu_mode.split(";")[1]+";"+secu_mode.split(";")[2];
		if(f.encry_type[3].checked == true)
			f.WscEncrypType2.value = 4;
		else if(f.encry_type[4].checked == true)
			f.WscEncrypType2.value = 8;
		if(f.authen_code_sele.options.selectedIndex == 3)
			f.AuthMode.value ="WPAPSK;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
			if(f.encry_type[3].checked == true)
				f.EncrypType.value = "TKIP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			else if(f.encry_type[4].checked == true)
				f.EncrypType.value = "AES;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
		else if(f.authen_code_sele.options.selectedIndex == 4)
			f.AuthMode.value ="WPA2PSK;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
			if(f.encry_type[3].checked == true)
				f.EncrypType.value = "TKIP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			else if(f.encry_type[4].checked == true)
				f.EncrypType.value = "AES;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			else 
				f.EncrypType.value = "TKIPAES;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
		else
			f.AuthMode.value ="WPAPSKWPA2PSK;"+auth_mode.split(";")[1]+";"+auth_mode.split(";")[2];
			if(f.encry_type[3].checked == true)
				f.EncrypType.value = "TKIP;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			else if(f.encry_type[4].checked == true)
				f.EncrypType.value = "AES;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
			else 
				f.EncrypType.value = "TKIPAES;"+encry_type.split(";")[1]+";"+encry_type.split(";")[2];
        if(f.wpa_key.value.length < 8)
			alert("
8!");
			f.wpa_key.focus();
			f.wpa_key.select();
			return;
		else if(/.*[^\x00-\xff]+.*$/.test(f.wpa_key.value))
			alert("
J!");
			f.wpa_key.select();
			return;
		if(f.wpa_key.value.length==64&&IsHex(f.wpa_key.value))
			alert("16
(0-9,a-f,A-F)
J!");
			f.wpa_key.focus();
			f.wpa_key.select();
			return;
		setCfg("WPAPSK1Temp",f.wpa_key.value);
		setCfg("WscNewKey2",f.wpa_key.value);
		//f.WscNewKey2.value = f.wpa_key.value;
	form2Cfg(f);
	if(option_mode ==2)
		subForm(f,'do_cmd.htm','DHCP',cPage);
	else
		subForm(f,'do_cmd.htm','WIRELESS_APCLI_SETUP',cPage);
	return true;
function check(obj){
  var test=obj.value.match(/[^A-Za-z0-9]/g);
  if(test!=null){
   obj.value="";
function checkSSID()
  var f=document.wireless_basic;
  if(/.*[^\x00-\xff]+.*$/.test(f.ssid.value))
	alert("SSID
J!");
	f.ssid.select();
  /*else if ((f.ssid.value.indexOf("'")!=-1)||(f.ssid.value.indexOf('"')!=-1)||
		   (f.ssid.value.indexOf("#")!=-1)||(f.ssid.value.indexOf("!")!=-1)||
		   (f.ssid.value.indexOf("^")!=-1)||(f.ssid.value.indexOf("|")!=-1))
   {
			alert("SSID
[' \" # ! ^ |],
J!");
			f.ssid.select();		
   }*/
