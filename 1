<script>
function setvalue()
	var h;
	var tmpi,tmpj;
	var sele_flag =0 ;
	var i = document.getElementById("aplist").rows.length;
	if (i != 1)
	for(h=0;h<i;h++)
		if(document.scan_fm.selectap[h].checked == true)
			tmpi=h;
			tmpj=document.scan_fm.selectap[h].value;
			sele_flag = 1;
			break;
	else if(i==1) 
		if(document.scan_fm.selectap.checked == true)
			tmpi=0;
			tmpj=document.scan_fm.selectap.value;
			sele_flag = 1;
	if(!sele_flag)
		alert("
AP!");
		return;
	window.opener.document.wds.ap_bssid1.value = tmpj.split(":")[0];
	window.opener.document.wds.ap_bssid2.value = tmpj.split(":")[1];
	window.opener.document.wds.ap_bssid3.value = tmpj.split(":")[2];
	window.opener.document.wds.ap_bssid4.value = tmpj.split(":")[3];
	window.opener.document.wds.ap_bssid5.value = tmpj.split(":")[4];
	window.opener.document.wds.ap_bssid6.value = tmpj.split(":")[5];
	window.close(); 
function init()
	var num = document.getElementById("aplist").rows.length;
	for(var j=0;j<num;j++)
		document.getElementById("ssid_"+j).innerHTML = document.getElementById("ssid_"+j).innerHTML.replace(eval("/ /gi"),'&nbsp;');
</script>
</head>
